# menu MENU_ID
#  func  LABEL FUNCTION KEYS
#  popup LABEL MENU_ID  KEYS
#  nop   LABEL
# end
#
# MENU_ID
#  Main:   Main menu
#  Select: Buffer selection menu

menu Main
	func "戻る                        (b)" PREV "Bb"
	func "進む                        (f)" NEXT "Ff"
	func "更新                        (r)" RELOAD "Rr"
	nop  "-------------------------------"
	func "このページをブックマーク    (m)" ADD_BOOKMARK "Mm"
	func "名前を付けてページを保存... (p)" SAVE "Pp"
	func "ページ URL を送信...        (e)" GOTO "Ee" "file:///cgi-bin/w3mplus?pass=$(PASS)&action=execute-env-var&subaction=redirect&redirect=mailto%3A%3Fbody%3D&variable=W3M_URL"
	nop  "-------------------------------"
	func "ページのソースを表示        (v)" VIEW "Vv"
	func "ページの情報を表示          (i)" INFO "Ii"
end

menu ContextMenuLink
	func "リンクを新しいタブで開く                   (t)" TAB_LINK "Tt"
	func "リンクを新しいウィンドウで開く             (w)" EXTERN_LINK "Ww" "w3m %s"
	func "リンクを新しいプライベートウィンドウで開く (p)" EXTERN_LINK "Pp" "w3m-private --link './.w3m/bookmark.html' --link './.w3mplus' -- %s"
	func "選択した URL を開く                        (o)" GOTO_LINK "Oo"
	nop  "----------------------------------------------"
	func "このリンクをブックマーク                   (l)" GOTO "Ll" "file:///cgi-bin/w3mplus?pass=$(PASS)&action=execute-env-var&subaction=goto&after=ADD_BOOKMARK&variable=W3M_CURRENT_LINK"
	func "名前を付けてリンク先を保存...              (k)" SAVE_LINK "Kk"
	func "リンクを送信...                            (d)" GOTO "Dd" "file:///cgi-bin/w3mplus?pass=$(PASS)&action=execute-env-var&subaction=redirect&redirect=mailto%3A%3Fbody%3D&variable=W3M_CURRENT_LINK"
	func "リンクの URL をコピー                      (a)" GOTO "Aa" "file:///cgi-bin/w3mplus?pass=$(PASS)&action=execute-env-var&subaction=yank&variable=W3M_CURRENT_LINK"
end

menu ContextMenuImage
	func "画像だけを表示               (i)" VIEW_IMAGE "Ii"
	func "画像の URL をコピー          (o)" GOTO "Oo" "file:///cgi-bin/w3mplus?pass=$(PASS)&action=execute-env-var&subaction=yank&variable=W3M_CURRENT_IMG"
	nop  "--------------------------------"
	func "名前を付けて画像を保存...    (v)" SAVE_IMAGE "Vv"
	func "画像の URL をメールで送信... (g)" GOTO "Gg" "file:///cgi-bin/w3mplus?pass=$(PASS)&action=execute-env-var&subaction=redirect&redirect=mailto%3A%3Fbody%3D&variable=W3M_CURRENT_IMG"
end

menu ContextMenuLinkImage
	func "リンクを新しいタブで開く           (t)" TAB_LINK "Tt"
	func "リンクを新しいウィンドウで開く     (w)" EXTERN_LINK "Ww" "w3m %s"
	func "新しいプライベートウィンドウで開く (p)" EXTERN_LINK "Pp" "w3m-private --link './.w3m/bookmark.html' --link './.w3mplus' -- %s"
	func "選択した URL をコピー              (O)" GOTO_LINK "O"
	nop  "--------------------------------------"
	func "このリンクをブックマーク           (l)" GOTO "Ll" "file:///cgi-bin/w3mplus?pass=$(PASS)&action=execute-env-var&subaction=goto&after=ADD_BOOKMARK&variable=W3M_CURRENT_LINK"
	func "名前を付けてリンク先を保存...      (k)" SAVE_LINK "Kk"
	func "リンクを送信...                    (d)" GOTO "Dd" "file:///cgi-bin/w3mplus?pass=$(PASS)&action=execute-env-var&subaction=redirect&redirect=mailto%3A%3Fbody%3D&variable=W3M_CURRENT_LINK"
	func "リンクの URL をコピー              (a)" GOTO "Aa" "file:///cgi-bin/w3mplus?pass=$(PASS)&action=execute-env-var&subaction=yank&variable=W3M_CURRENT_LINK"
	nop  "--------------------------------------"
	func "画像だけを表示                     (i)" VIEW_IMAGE "Ii"
	func "画像の URL をコピー                (o)" GOTO "Oo" "file:///cgi-bin/w3mplus?pass=$(PASS)&action=execute-env-var&subaction=yank&variable=W3M_CURRENT_IMG"
	nop  "--------------------------------------"
	func "名前を付けて画像を保存...          (v)" SAVE_IMAGE "Vv"
	func "画像の URL をメールで送信...       (g)" GOTO "Gg" "file:///cgi-bin/w3mplus?pass=$(PASS)&action=execute-env-var&subaction=redirect&redirect=mailto%3A%3Fbody%3D&variable=W3M_CURRENT_IMG"
end

menu ContextMenuTab
	func "新しいタブ             (n)" NEW_TAB "Nn"
	func "タブを再読み込み       (t)" RELOAD "Tt"
	func "新しいウィンドウへ移動 (w)" EXTERN "Ww" "w3m %s"
	nop  "--------------------------"
	func "閉じたタブを元に戻す   (u)" GOTO "u" "file:///cgi-bin/w3mplus?pass=$(PASS)&action=undo-tab"
	func "タブを閉じる           (c)" COMMAND "Cc" "TAB_RIGHT; GOTO file:///cgi-bin/w3mplus?pass=$(PASS)&action=close-tab"
	func "他のタブをすべて閉じる (o)" COMMAND "Oo" "TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; PREV_TAB; CLOSE_TAB; CLOSE_TAB; CLOSE_TAB; CLOSE_TAB; CLOSE_TAB; CLOSE_TAB; CLOSE_TAB; CLOSE_TAB; CLOSE_TAB; CLOSE_TAB; CLOSE_TAB; CLOSE_TAB; CLOSE_TAB; CLOSE_TAB; CLOSE_TAB; CLOSE_TAB; CLOSE_TAB; CLOSE_TAB; CLOSE_TAB; CLOSE_TAB"
	nop  "--------------------------"
	func "タブを左に移動         (l)" TAB_LEFT "l"
	func "タブを右に移動         (r)" TAB_RIGHT "r"
	nop  "--------------------------"
	func "タブを最も左に移動     (L)" COMMAND "L" "TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT"
	func "タブを最も右に移動     (R)" COMMAND "R" "TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT"
	nop  "--------------------------"
	func "すべてのタブを表示     (a)" TAB_MENU "Aa"
end

menu ContextMenuSearch
	func  "Bing" COMMAND "" "SETENV W3MPLUS_SEARCH_ENGINE=bing; SET_OPTION dictcommand=file:///cgi-bin/w3mplus?pass=$(PASS)&action=dict-word&designator=all&query=; DICT_WORD"
	func  "Google" COMMAND "" "SETENV W3MPLUS_SEARCH_ENGINE=google; SET_OPTION dictcommand=file:///cgi-bin/w3mplus?pass=$(PASS)&action=dict-word&designator=all&query=; DICT_WORD"
	func  "DuckDuckGo" COMMAND "" "SETENV W3MPLUS_SEARCH_ENGINE=duckduckgo; SET_OPTION dictcommand=file:///cgi-bin/w3mplus?pass=$(PASS)&action=dict-word&designator=all&query=; DICT_WORD"
	func  "Startpage" COMMAND "" "SETENV W3MPLUS_SEARCH_ENGINE=startpage; SET_OPTION dictcommand=file:///cgi-bin/w3mplus?pass=$(PASS)&action=dict-word&designator=all&query=; DICT_WORD"
	func  "Twitter" COMMAND "" "SETENV W3MPLUS_SEARCH_ENGINE=twitter; SET_OPTION dictcommand=file:///cgi-bin/w3mplus?pass=$(PASS)&action=dict-word&designator=all&query=; DICT_WORD"
	func  "Wikipedia (en)" COMMAND "" "SETENV W3MPLUS_SEARCH_ENGINE=wikipedia-en; SET_OPTION dictcommand=file:///cgi-bin/w3mplus?pass=$(PASS)&action=dict-word&designator=all&query=; DICT_WORD"
	func  "Wikipedia (ja)" COMMAND "" "SETENV W3MPLUS_SEARCH_ENGINE=wikipedia-ja; SET_OPTION dictcommand=file:///cgi-bin/w3mplus?pass=$(PASS)&action=dict-word&designator=all&query=; DICT_WORD"
	popup "その他の検索" ContextMenuSearchOther ""
end

menu ContextMenuSearchOther
	func "Bing Image" COMMAND "" "SETENV W3MPLUS_SEARCH_ENGINE=bing-image; SET_OPTION dictcommand=file:///cgi-bin/w3mplus?pass=$(PASS)&action=dict-word&designator=all&query=; DICT_WORD"
	func "Bing Video" COMMAND "" "SETENV W3MPLUS_SEARCH_ENGINE=bing-video; SET_OPTION dictcommand=file:///cgi-bin/w3mplus?pass=$(PASS)&action=dict-word&designator=all&query=; DICT_WORD"
	func "Bing News" COMMAND "" "SETENV W3MPLUS_SEARCH_ENGINE=bing-news; SET_OPTION dictcommand=file:///cgi-bin/w3mplus?pass=$(PASS)&action=dict-word&designator=all&query=; DICT_WORD"
	func "GitHub" COMMAND "" "SETENV W3MPLUS_SEARCH_ENGINE=github; SET_OPTION dictcommand=file:///cgi-bin/w3mplus?pass=$(PASS)&action=dict-word&designator=all&query=; DICT_WORD"
	func "Google Image" COMMAND "" "SETENV W3MPLUS_SEARCH_ENGINE=google-image; SET_OPTION dictcommand=file:///cgi-bin/w3mplus?pass=$(PASS)&action=dict-word&designator=all&query=; DICT_WORD"
	func "Google Video" COMMAND "" "SETENV W3MPLUS_SEARCH_ENGINE=google-video; SET_OPTION dictcommand=file:///cgi-bin/w3mplus?pass=$(PASS)&action=dict-word&designator=all&query=; DICT_WORD"
	func "Google News" COMMAND "" "SETENV W3MPLUS_SEARCH_ENGINE=google-news; SET_OPTION dictcommand=file:///cgi-bin/w3mplus?pass=$(PASS)&action=dict-word&designator=all&query=; DICT_WORD"
	func "pkgs.org" COMMAND "" "SETENV W3MPLUS_SEARCH_ENGINE=pkgs.org; SET_OPTION dictcommand=file:///cgi-bin/w3mplus?pass=$(PASS)&action=dict-word&designator=all&query=; DICT_WORD"
	func "Yandex" COMMAND "" "SETENV W3MPLUS_SEARCH_ENGINE=yandex; SET_OPTION dictcommand=file:///cgi-bin/w3mplus?pass=$(PASS)&action=dict-word&designator=all&query=; DICT_WORD"
	func "Yandex Image" COMMAND "" "SETENV W3MPLUS_SEARCH_ENGINE=yandex-image; SET_OPTION dictcommand=file:///cgi-bin/w3mplus?pass=$(PASS)&action=dict-word&designator=all&query=; DICT_WORD"
	func "Yandex Video" COMMAND "" "SETENV W3MPLUS_SEARCH_ENGINE=yandex-video; SET_OPTION dictcommand=file:///cgi-bin/w3mplus?pass=$(PASS)&action=dict-word&designator=all&query=; DICT_WORD"
	func "YouTube" COMMAND "" "SETENV W3MPLUS_SEARCH_ENGINE=youtube; SET_OPTION dictcommand=file:///cgi-bin/w3mplus?pass=$(PASS)&action=dict-word&designator=all&query=; DICT_WORD"
end

menu Menu
	popup "ファイル     (f)" MenuFile "Ff"
	popup "編集         (e)" MenuEdit "Ee"
	popup "表示         (v)" MenuView "Vv"
	popup "履歴         (s)" MenuHistory "Ss"
	popup "ブックマーク (b)" MenuBookmarks "Bb"
	popup "ツール       (t)" MenuTools "Tt"
	popup "ヘルプ       (h)" MenuHelp "Hh"
end

menu MenuFile
	func "新しいタブ                     (t)" NEW_TAB "Tt"
	func "新しいウィンドウ               (n)" EXTERN "Nn" "w3m ${WWW_HOME-about:home} #"
	func "新しいプライベートウィンドウ   (w)" EXTERN "Ww" "w3m-private --link './.w3m/bookmark.html' --link './.w3mplus' -- 'about:private' #"
	func "ファイルを開く...              (o)" LOAD "Oo"
	nop  "----------------------------------"
	func "名前を付けてページを保存...    (a)" DOWNLOAD "Aa"
	func "ページの URL をメールで送信... (e)" GOTO "Ee" "file:///cgi-bin/w3mplus?pass=$(PASS)&action=execute-env-var&subaction=redirect&redirect=mailto%3A%3Fbody%3D&variable=W3M_URL"
	nop  "----------------------------------"
	func "印刷                           (p)" PRINT "Pp"
	nop  "----------------------------------"
	func "設定の再読み込み               (r)" REINIT "Rr"
	func "中断                           (s)" SUSPEND "Ss"
	func "終了                           (q)" EXIT "Qq"
end

menu MenuEdit
	func "元に戻す   (u)" UNDO "Uu"
	func "やり直し   (r)" REDO "Rr"
	nop  "--------------"
	func "コピー     (c)" GOTO "Cc" "file:///cgi-bin/w3mplus?pass=$(PASS)&action=execute-env-var&subaction=yank&variable=W3M_CURRENT_WORD"
	func "マーク     (m)" MARK "Mm"
	nop  "--------------"
	func "検索       (f)" ISEARCH "Ff"
	func "ハイライト (h)" REG_MARK "Hh"
end

menu MenuView
	popup "サイドバー           (e)" MenuSidebar "Ee"
	nop   "------------------------"
	popup "ズーム               (z)" MenuZoom "Zz"
	func  "文字エンコーディング (c)" CHARSET "Cc"
	popup "スタイルと色調       (s)" MenuStyle "Ss"
	nop   "------------------------"
	func  "ナビゲーションリンク (n)" LINK_MENU "Nn"
	func  "すべてのリンクを表示 (l)" LIST_MENU "Ll"
	nop   "------------------------"
	func  "ページャーを開く     (p)" COMMAND "Pp" "SET_OPTION \"editor=${PAGER:-less}\"; EDIT_SCREEN; SET_OPTION \"editor=${VISUAL:-${EDITOR:-vi --}}\""
	func  "すべてのタブを表示   (a)" TAB_MENU "Aa"
end

menu MenuSidebar
	func "ブックマーク (b)" GOTO "Bb" "file:///cgi-bin/w3mplus?pass=$(PASS)&action=sidebar-bookmark"
	func "履歴         (h)" GOTO "Hh" "file:///cgi-bin/w3mplus?pass=$(PASS)&action=sidebar-history"
end

menu MenuZoom
	func "拡大     (i)" GOTO "Ii" "file:///cgi-bin/w3mplus?pass=$(PASS)&action=zoom&zoom=%2B%3D20"
	func "縮小     (o)" GOTO "Oo" "file:///cgi-bin/w3mplus?pass=$(PASS)&action=zoom&zoom=-%3D20"
	nop  "------------"
	func "リセット (r)" GOTO "Rr" "file:///cgi-bin/w3mplus?pass=$(PASS)&action=zoom&zoom=100"
end

menu MenuStyle
	func "lynx         (l)" COMMAND "Ll" "SET_OPTION display_ins_del=1; SET_OPTION color=1; SET_OPTION basic_color=terminal; SET_OPTION anchor_color=green; SET_OPTION image_color=yellow; SET_OPTION form_color=white; SET_OPTION mark_color=cyan; SET_OPTION bg_color=terminal; SET_OPTION active_style=1; SET_OPTION active_color=yellow; SET_OPTION visited_anchor=0"
	func "モノクローム (m)" COMMAND "Mm" "SET_OPTION display_ins_del=1; SET_OPTION color=0; SET_OPTION basic_color=terminal; SET_OPTION bg_color=terminal"
	func "リセット     (r)" COMMAND "Rr" "SET_OPTION display_ins_del=2; SET_OPTION color=1; SET_OPTION basic_color=terminal; SET_OPTION anchor_color=blue; SET_OPTION image_color=green; SET_OPTION form_color=red; SET_OPTION mark_color=yellow; SET_OPTION bg_color=terminal; SET_OPTION active_style=1; SET_OPTION active_color=cyan; SET_OPTION visited_anchor=1; SET_OPTION visited_color=magenta"
end

menu MenuHistory
	func  "すべての履歴を表示 (h)" HISTORY "Hh"
	func  "タブの履歴を表示   (b)" SELECT_MENU "Bb"
	popup "最近閉じたタブ     (u)" MenuUndoClosedTabs "Uu"
end

menu MenuUndoClosedTabs
	func "1個のタブを復元      (u)" GOTO "Uu" "file:///cgi-bin/w3mplus?pass=$(PASS)&action=undo-tab"
	func "5個のタブを復元      (f)" GOTO "Ff" "file:///cgi-bin/w3mplus?pass=$(PASS)&action=undo-tab&count=-5"
	func "10個のタブを復元     (t)" GOTO "Tt" "file:///cgi-bin/w3mplus?pass=$(PASS)&action=undo-tab&count=-10"
	nop  "------------------------"
	func "10分以内のタブを復元 (1)" GOTO "1" "file:///cgi-bin/w3mplus?pass=$(PASS)&action=undo-tab&count=%40-600"
	func "30分以内のタブを復元 (3)" GOTO "3" "file:///cgi-bin/w3mplus?pass=$(PASS)&action=undo-tab&count=%40-1800"
	func "60分以内のタブを復元 (6)" GOTO "6" "file:///cgi-bin/w3mplus?pass=$(PASS)&action=undo-tab&count=%40-3600"
	nop  "------------------------"
	func "すべてのタブを復元   (a)" GOTO "Aa" "file:///cgi-bin/w3mplus?pass=$(PASS)&action=undo-tab&count=-0"
end

menu MenuBookmarks
	func "このページをブックマーク (m)" ADD_BOOKMARK "Mm"
	func "ブックマークを表示       (v)" VIEW_BOOKMARK "Vv"
end

menu MenuTools
	func  "ダウンロード  (d)" DOWNLOAD_LIST "Dd"
	func  "Cookies       (c)" COOKIE "Cc"
	func  "サイト別設定  (m)" GOTO "Mm" "about:permissions"
	nop   "-----------------"
	popup "ウェブ開発    (w)" MenuWebDeveloper "Ww"
	func  "ページの情報  (i)" INFO "Ii"
	nop   "-----------------"
	popup "Cookie の管理 (k)" MenuCookieManager "Kk"
	popup "User Agent    (u)" MenuUserAgent "Uu"
	func  "httpsのみ     (s)" GOTO "Ss" "file:///cgi-bin/w3mplus?pass=$(PASS)&action=toggle-https-everywhere"
	func  "設定          (p)" OPTIONS "Pp"
end

menu MenuWebDeveloper
	func "ウェブコンソール (w)" PIPE_BUF "Ww"
	func "ネットワーク     (n)" LOAD "Nn" "~/.w3m/request.log"
	nop  "--------------------"
	func "開発ツールバー   (v)" COMMAND "Vv"
	func "スクラッチパッド (s)" EXEC_SHELL "Ss"
	func "ページのソース   (o)" VIEW "Oo"
	func "エラーコンソール (c)" MSGS "Cc"
end

menu MenuCookieManager
	nop  "-- ドメインをブラックリストに適用 --"
	func "サブドメインも適用               (b)" GOTO "b" "file:///cgi-bin/w3mplus?pass=$(PASS)&action=cookie-config&reject=toggle&subdomain=1"
	func "ページのドメインのみ適用         (B)" GOTO "B" "file:///cgi-bin/w3mplus?pass=$(PASS)&action=cookie-config&reject=toggle"
	nop  "-- ドメインをホワイトリストに適用 --"
	func "サブドメインも適用               (w)" GOTO "w" "file:///cgi-bin/w3mplus?pass=$(PASS)&action=cookie-config&accept=toggle&subdomain=1"
	func "ページのドメインのみ適用         (W)" GOTO "W" "file:///cgi-bin/w3mplus?pass=$(PASS)&action=cookie-config&accept=toggle
end

menu MenuUserAgent
	nop  "-- CLI Web ブラウザー ----"
	func "w3m                    (3)" SET_OPTION "3" "user_agent="
	func "lynx                   (l)" SET_OPTION "l" "user_agent=Lynx/2.8.9rel.1 libwww-FM/2.14 SSL-MM/1.4.1 GNUTLS/3.6.5"
	nop  "-- GUI Web ブラウザー ----"
	func "Internet Explorer      (i)" SET_OPTION "i" "user_agent=Mozilla/5.0 (Windows NT 6.3; Win64; x64; Trident/7.0; rv:11.0) like Gecko"
	func "Microsoft Edge         (e)" SET_OPTION "e" "user_agent=Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/70.0.35g38.102 Safari/537.36 Edge/18.18363"
	func "Safari                 (s)" SET_OPTION "s" "user_agent=Mozilla/5.0 (Macintosh; Intel Mac OS X 10_9_3) AppleWebKit/537.75.14 (KHTML, like Gecko) Version/7.0.3 Safari/7046A194A"
	func "Google Chrome          (c)" SET_OPTION "c" "user_agent=Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/79.0.3945.79 Safari/537.36"
	func "Mozilla Firefox        (f)" SET_OPTION "f" "user_agent=Mozilla/5.0 (Windows NT 10.0; Win64; x64; rv:68.0) Gecko/20100101 Firefox/68.0"
	func "PaleMoon               (p)" SET_OPTION "p" "user_agent=Mozilla/5.0 (Windows NT 10.0; Win64; x64; rv:68.9) Goanna/4.4 PaleMoon/28.8.0"
	func "NetSurf                (n)" SET_OPTION "n" "user_agent=NetSurf/3.6 (Linux; x86_64)"
	nop  "-- モバイル Web ブラウザー"
	func "android (Chrome)       (a)" SET_OPTION "a" "user_agent=Mozilla/5.0 (Linux; U; Android 6.0) AppleWebKit/537.16 (KHTML, like Gecko) Version/4.0 Mobile Safari/537.16 Chrome/33.0.0.0"
	func "iPhone  (Safari)       (i)" SET_OPTION "i" "user_agent=Mozilla/5.0 (iPhone; CPU iPhone OS 13_2_3 like Mac OS X) AppleWebKit/605.1.15 (KHTML, like Gecko) Version/13.0.3 Mobile/15E148 Safari/604.1"
	func "Firefox for Mobile     (F)" SET_OPTION "F" "user_agent=Mozilla/5.0 (Android 6.0.1; Mobile; rv:68.0) Gecko/68.0 Firefox/68.0"
	func "Windows 10 Mobile      (w)" SET_OPTION "w" "user_agent=Mozilla/5.0 (Windows Phone 10.0; Android 6.0.1; Microsoft; Lumia 950 XL) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/51.0.2704.79 Mobile Safari/537.36 Edge/14.14393"
	nop  "-- Web クローラー --------"
	func "Googlebot              (g)" SET_OPTION "g" "user_agent=Mozilla/5.0 (compatible; Googlebot/2.1; +http://www.google.com/bot.html)"
	func "Bingbot                (b)" SET_OPTION "b" "user_agent=Mozilla/5.0 (compatible; bingbot/2.0; +http://www.bing.com/bingbot.htm)"
	func "Baiduspider            (B)" SET_OPTION "B" "user_agent=Mozilla/5.0 (compatible; Baiduspider/2.0; +http://www.baidu.com/search/spider.html)"
	nop  "-- ダウンロードツール ----"
	func "Wget                   (W)" SET_OPTION "W" "user_agent=Wget/1.20.1 (linux-gnueabihf)"
	func "curl                   (C)" SET_OPTION "C" "user_agent=curl/7.64.0"
end

menu MenuHelp
	func "w3m ヘルプ   (h)" HELP "Hh"
	func "Man w3m      (m)" EXEC_SHELL "Mm" "man w3m"
	nop  "----------------"
	func "セーフモード (s)" EXTERN "Ss" "w3m -config \"${HOME}/.w3m/default/config\" %s"
	nop  "----------------"
	func "w3m について (a)" VERSION "Aa"
end

menu ExtendedHints
	func ";  リンクをフォーカス" MOVE_LIST_MENU ";"
	func "?  行の情報を表示" LINE_INFO "?"
	func "s  リンク先を保存" SAVE_LINK "s"
	func "o  リンクを開く" GOTO_LINK "o"
	func "t  リンクを新しいタブで開く" TAB_LINK "t"
	func "b  バックグラウンドタブでリンクを開く" GOTO "b" "file:///cgi-bin/w3mplus?pass=$(PASS)&action=execute-env-var&subaction=goto&tab=open-newtab&after=PREV_TAB&variable=W3M_CURRENT_LINK&redirect=2"
	func "w  リンクを新しいウィンドウで開く" EXTERN_LINK "w" "w3m %s"
	func "O  Generate a ':o URL' prompt" GOTO "O" "file:///cgi-bin/w3mplus?pass=$(PASS)&action=link-goto"
	func "T  Generate a ':t URL' prompt" GOTO "T" "file:///cgi-bin/w3mplus?pass=$(PASS)&action=link-tabgoto"
	func "W  リンクを外部のブラウザーで開く" EXTERN_LINK "W"
	func "a  ブックマークに追加" GOTO "a" "file:///cgi-bin/w3mplus?pass=$(PASS)&action=execute-env-var&subaction=goto&after=ADD_BOOKMARK&variable=W3M_CURRENT_LINK"
	func "S  リンクの URL を検索" GOTO "S" "file:///cgi-bin/w3mplus?pass=$(PASS)&action=execute-env-var&subaction=redirect&variable=W3M_CURRENT_LINK"
	func "v  リンク先のソースを表示" GOTO "v" "file:///cgi-bin/w3mplus?pass=$(PASS)&action=execute-env-var&subaction=goto&after=VIEW&variable=W3M_CURRENT_LINK"
	func "V  リンク先のソースを外部エディターで表示" GOTO "V" "file:///cgi-bin/w3mplus?pass=$(PASS)&action=execute-env-var&subaction=goto&after=COMMAND%20VIEW%3B%20EDIT_SCREEN%3B%20VIEW&variable=W3M_CURRENT_LINK"
	func "y  リンクの URL をヤンク" GOTO "y" "file:///cgi-bin/w3mplus?pass=$(PASS)&action=execute-env-var&subaction=yank&variable=W3M_CURRENT_LINK"
	func "Y  リンクテキストをヤンク" GOTO "Y" "file:///cgi-bin/w3mplus?pass=$(PASS)&action=execute-env-var&subaction=goto&after=COMMAND%20VIEW%3B%20EDIT_SCREEN%3B%20VIEW&variable=W3M_CURRENT_WORD"
	func "c  コンテキストメニューを開く" MENU "c"
	func "i  画像を表示" VIEW_IMAGE "i"
	func "I  画像を保存" SAVE_IMAGE "I"
end

menu Commands
	func  "!  シェルコマンドを実行" EXEC_SHELL "!"
	func  "\"  レジスタファイルを表示" GOTO "\"" "file:///cgi-bin/w3mplus?pass=$(PASS)&action=show-register"
	func  ":  w3m のコマンドを実行" COMMAND ":"
	func  "b  ブックマークを開く" VIEW_BOOKMARK "b"
	func  "c  サイトのCookieアクセス許可を変更" COOKIE "c"
	popup "C  カラースキームを読み込む" MenuStyle "C"
	func  "d  ダウンロードリストを表示" DOWNLOAD_LIST "d"
	func  "D  指定されたURLパターンに一致するオートコマンドを適用" GOTO "D" "file:///cgi-bin/w3mplus?pass=$(PASS)&action=execute-user-command&key=manual"
	func  "e  w3m メニューを表示" MENU "e" Menu
	func  "f  HTML フレームの表示の切り替え" FRAME "f"
	func  "G  メッセージを表示" MSGS "G"
	func  "h  入門用のヘルプを開く" HELP "h"
	func  "H  最近訪れた URL を表示" HISTORY "H"
	func  "j  ジャンプリストを表示" SELECT_MENU "j"
	popup "l  リストコマンドを表示" LCommands "l"
	func  "L  環境変数を設定" SETENV "L"
	func  "m  ローカルマークを表示" GOTO "m" "file:///cgi-bin/w3mplus?pass=$(PASS)&action=show-local-mark"
	func  "M  URL マークを表示" GOTO "M" "file:///cgi-bin/w3mplus?pass=$(PASS)&action=show-url-mark"
	func  "n  Map a key sequence in Normal mode" DEFINE_KEY "n"
	func  "o  現在のタブで URL を開く" COMMAND "o" "SET_OPTION default_url=0; GOTO"
	func  "q  w3m を終了" QUIT "q"
	func  "Q  すべてのQuickMarkを表示する" GOTO "Q" "file:///cgi-bin/w3mplus?pass=$(PASS)&action=show-quick-mark"
	func  "p  w3m の設定を表示" OPTIONS "p"
	func  "r  シェルコマンドを実行して出力を表示" PIPE_SHELL "r"
	func  "s  w3m のオプションを設定" SET_OPTION "s"
	func  "S  シェルを開始" EXEC_SHELL "S" "${SHELL}"
	func  "t  新しいタブで URL を開く" COMMAND "t" "SET_OPTION default_url=0; TAB_GOTO"
	func  "u  最近閉じたタブの一覧" GOTO "u" "file:///cgi-bin/w3mplus?pass=$(PASS)&action=show-tab-history"
	func  "v  バージョン情報を表示" VERSION "v"
	func  "w  現在のページをディスクに保存" DOWNLOAD "w"
	func  "W  現在のページを印刷" PRINT "W"
	func  "x  w3m を終了" EXIT "x"
	popup "z  Set zoom value of current web page" Zoom "z"
end

menu LCommands
	popup "c  List all Ex commands along with their short descriptions" Commands "c"
	func  "K  List all mappings along with their short descriptions" HELP "k"
	func  "o  List all options along with their short descriptions" PIPE_SHELL "o" "w3m -show-option"
end

menu Zoom
	popup "0    0% -  90%" Zoom_0Percents "0"
	popup "1  100% - 190%" Zoom_100Percents "1"
	popup "2  200% - 290%" Zoom_200Percents "2"
	popup "3  300% - 390%" Zoom_300Percents "3"
	popup "4  400% - 490%" Zoom_400Percents "4"
	popup "+  拡大" ZoomIn "+"
	popup "-  縮小" ZoomOut "-"
end

