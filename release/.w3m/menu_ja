	func "t  移動 (新しいタブ)" GOTO "t" "file:///cgi-bin/w3mplus?pass=$(PASS)&action=execute-env-var&subaction=goto&variable=W3M_URL&tab=newtab"
	func "p  検索 (現在のタブ)" GOTO "p" "file:///cgi-bin/w3mplus?pass=$(PASS)&action=execute-env-var&subaction=redirect&variable=W3M_URL"
	func "P  検索 (新しいタブ)" GOTO "P" "file:///cgi-bin/w3mplus?pass=$(PASS)&action=execute-env-var&subaction=redirect&variable=W3M_URL&tab=open-newtab"
