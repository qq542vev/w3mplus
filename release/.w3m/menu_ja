menu Filter_3Line
	func "j  下の3行" GOTO "j" "file:///cgi-bin/w3mplus?pass=$(PASS)&action=select-line&subaction=filter&number=%2B3"
	func "k  上の3行" GOTO "k" "file:///cgi-bin/w3mplus?pass=$(PASS)&action=select-line&subaction=filter&number=-3"
	func "*  上下の3行" GOTO "*" "file:///cgi-bin/w3mplus?pass=$(PASS)&action=select-line&subaction=filter&number=%2B-3"
end

