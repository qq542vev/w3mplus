	func "s  メールで送信" GOTO "s" "file:///cgi-bin/w3mplus?pass=$(PASS)&action=get-register&key=u&subaction=redirect&redirect=mailto%3A%3Fbody%3D"
	func "S  表示" GOTO "S" "file:///cgi-bin/w3mplus?pass=$(PASS)&action=get-register&key=u&subaction=read-shell"
	func "y  URLをヤンク" COMMAND "y" "GOTO file:///cgi-bin/w3mplus?pass=$(PASS)&action=set-register&key=u&value=W3M_URL"
	func "Y  語句をヤンク" COMMAND "Y" "GOTO file:///cgi-bin/w3mplus?pass=$(PASS)&action=set-register&key=u&value=W3M_CURRENT_WORD"
end

