menu Filter
	func  "!,h,l  現在の行" GOTO "!hl" "file:///cgi-bin/w3mplus?pass=$(PASS)&action=select-line&subaction=filter&number=0"
	func  "j      下の1行" GOTO "j" "file:///cgi-bin/w3mplus?pass=$(PASS)&action=select-line&subaction=filter&number=%2B1"
	func  "k      上の1行" GOTO "k" "file:///cgi-bin/w3mplus?pass=$(PASS)&action=select-line&subaction=filter&number=-1"
	func  "g      すべての上の行" GOTO "g" "file:///cgi-bin/w3mplus?pass=$(PASS)&action=select-line&subaction=filter&number=1"
	func  "G      すべての下の行" GOTO "G" "file:///cgi-bin/w3mplus?pass=$(PASS)&action=select-line&subaction=filter&number=%24"
	func  "*      すべての行" PIPE_BUF "*"
	func  "V      Select lines" GOTO "V" "file:///cgi-bin/w3mplus?pass=$(PASS)&action=visual-mode&subaction=filter"
	nop   "---------------------"
	popup "1      1行" Filter_1Line "1"
	popup "2      2行" Filter_2Line "2"
	popup "3      3行" Filter_3Line "3"
	popup "4      4行" Filter_4Line "4"
	popup "5      5行" Filter_5Line "5"
	popup "6      6行" Filter_6Line "6"
	popup "7      7行" Filter_7Line "7"
	popup "8      8行" Filter_8Line "8"
	popup "9      9行" Filter_9Line "9"
end

menu Filter_1Line
	func "j  下の1行" GOTO "j" "file:///cgi-bin/w3mplus?pass=$(PASS)&action=select-line&subaction=filter&number=%2B1"
	func "k  上の1行" GOTO "k" "file:///cgi-bin/w3mplus?pass=$(PASS)&action=select-line&subaction=filter&number=-1"
	func "*  上下の1行" GOTO "*" "file:///cgi-bin/w3mplus?pass=$(PASS)&action=select-line&subaction=filter&number=%2B-1"
end

menu Filter_2Line
	func "j  下の2行" GOTO "j" "file:///cgi-bin/w3mplus?pass=$(PASS)&action=select-line&subaction=filter&number=%2B2"
	func "k  上の2行" GOTO "k" "file:///cgi-bin/w3mplus?pass=$(PASS)&action=select-line&subaction=filter&number=-2"
