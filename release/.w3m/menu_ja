menu ToggleOption
	func "b  table の border 値を(解釈 / 無視)" COMMAND "b" "SET_OPTION display_borders=toggle; RESHAPE"
	func "c  HTTP cookie を(使用 / 不使用)" SET_OPTION "c" "use_cookie=toggle"
	func "f  HTML フレームを(自動 / 手動)で表示" SET_OPTION "f" "frame=toggle"
	func "h  履歴機能を(有効 / 無効)" SET_OPTION "h" "save_hist=toggle"
	func "i  インライン画像を(表示 / 非表示)" SET_OPTION "i" "display_image=toggle"
	func "I  大文字小文字を(区別して / 同一で)検索" SET_OPTION "I" "ignorecase_search=toggle"
	func "m  マウス機能を(有効 / 無効)" MOUSE_TOGGLE "m"
	func "M  (モノクローム / カラー)で表示" SET_OPTION "M" "color=toggle"
	func "n  行番号を(表示 / 非表示)" SET_OPTION "n" "show_lnum=toggle"
	func "p  ネットワークプロキシを(使用 / 不使用)" SET_OPTION "p" "use_proxy=toggle"
	func "r  HTTP リファラを(送信 / 非送信)" SET_OPTION "r" "no_referer=toggle"
	func "u  ページ内の URL をリンクに(変換 / 非変換)" SET_OPTION "u" "mark_all_pages=toggle"
	func "w  テキストファイルの行折り返しを(有効/ 無効)" COMMAND "w" "SET_OPTION fold_line=toggle; RESHAPE"
	func "W  折り返し検索を(有効 / 無効)" WRAP_TOGGLE "W"
end

menu Register_0
	func "o  移動 (現在のタブ)" GOTO "o" "file:///cgi-bin/w3mplus?pass=$(PASS)&action=get-register&key=0&subaction=goto"
	func "t  移動 (新しいタブ)" GOTO "t" "file:///cgi-bin/w3mplus?pass=$(PASS)&action=get-register&key=0&subaction=goto&tab=newtab"
	func "p  検索 (現在のタブ)" GOTO "p" "file:///cgi-bin/w3mplus?pass=$(PASS)&action=get-register&key=0&subaction=redirect"
	func "P  検索 (新しいタブ)" GOTO "P" "file:///cgi-bin/w3mplus?pass=$(PASS)&action=get-register&key=0&subaction=redirect&tab=open-newtab"
	func "s  メールで送信" GOTO "s" "file:///cgi-bin/w3mplus?pass=$(PASS)&action=get-register&key=0&subaction=redirect&redirect=mailto%3A%3Fbody%3D"
	func "S  表示" GOTO "S" "file:///cgi-bin/w3mplus?pass=$(PASS)&action=get-register&key=0&subaction=read-shell"
	func "y  URLをヤンク" COMMAND "y" "GOTO file:///cgi-bin/w3mplus?pass=$(PASS)&action=set-register&key=0&value=W3M_URL"
	func "Y  語句をヤンク" COMMAND "Y" "GOTO file:///cgi-bin/w3mplus?pass=$(PASS)&action=set-register&key=0&value=W3M_CURRENT_WORD"
end

