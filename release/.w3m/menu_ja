menu Switchcase
	func  "~,h,l  現在の行" GOTO "~hl" "file:///cgi-bin/w3mplus?pass=$(PASS)&action=select-line&subaction=switchcase&number=0"
	func  "j      下の1行" GOTO "j" "file:///cgi-bin/w3mplus?pass=$(PASS)&action=select-line&subaction=switchcase&number=%2B1"
	func  "k      上の1行" GOTO "k" "file:///cgi-bin/w3mplus?pass=$(PASS)&action=select-line&subaction=switchcase&number=-1"
	func  "g      すべての上の行" GOTO "g" "file:///cgi-bin/w3mplus?pass=$(PASS)&action=select-line&subaction=switchcase&number=1"
	func  "G      すべての下の行" GOTO "G" "file:///cgi-bin/w3mplus?pass=$(PASS)&action=select-line&subaction=switchcase&number=%24"
	func  "*      すべての行" GOTO "*" "file:///cgi-bin/w3mplus?pass=$(PASS)&action=select-line&subaction=switchcase&line=&number=%24"
	func  "V      Select lines" GOTO "V" "file:///cgi-bin/w3mplus?pass=$(PASS)&action=visual-mode&subaction=switchcase"
	nop   "---------------------"
	popup "1      1行" Switchcase_1Line "1"
	popup "2      2行" Switchcase_2Line "2"
	popup "3      3行" Switchcase_3Line "3"
	popup "4      4行" Switchcase_4Line "4"
	popup "5      5行" Switchcase_5Line "5"
	popup "6      6行" Switchcase_6Line "6"
	popup "7      7行" Switchcase_7Line "7"
	popup "8      8行" Switchcase_8Line "8"
	popup "9      9行" Switchcase_9Line "9"
end

