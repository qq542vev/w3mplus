menu Operatorfunc_
	func  "@,h,l  現在の行" GOTO "@hl" "file:///cgi-bin/w3mplus?pass=$(PASS)&action=select-line&subaction=operatorfunc&number=0"
	func  "j      下の1行" GOTO "j" "file:///cgi-bin/w3mplus?pass=$(PASS)&action=select-line&subaction=operatorfunc&number=%2B1"
	func  "k      上の1行" GOTO "k" "file:///cgi-bin/w3mplus?pass=$(PASS)&action=select-line&subaction=operatorfunc&number=-1"
	func  "g      すべての上の行" GOTO "g" "file:///cgi-bin/w3mplus?pass=$(PASS)&action=select-line&subaction=operatorfunc&number=1"
	func  "G      すべての下の行" GOTO "G" "file:///cgi-bin/w3mplus?pass=$(PASS)&action=select-line&subaction=operatorfunc&number=%24"
	func  "*      すべての行" GOTO "*" "file:///cgi-bin/w3mplus?pass=$(PASS)&action=select-line&subaction=operatorfunc&line=&number=%24"
	func  "V      選択した行" GOTO "V" "file:///cgi-bin/w3mplus?pass=$(PASS)&action=visual-mode&subaction=operatorfunc"
	nop   "---------------------"
	popup "1      1行" Operatorfunc_1Line "1"
	popup "2      2行" Operatorfunc_2Line "2"
	popup "3      3行" Operatorfunc_3Line "3"
	popup "4      4行" Operatorfunc_4Line "4"
	popup "5      5行" Operatorfunc_5Line "5"
	popup "6      6行" Operatorfunc_6Line "6"
	popup "7      7行" Operatorfunc_7Line "7"
	popup "8      8行" Operatorfunc_8Line "8"
	popup "9      9行" Operatorfunc_9Line "9"
end

