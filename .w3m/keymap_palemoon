#### Page Up key ####

keymap "^[[5~" NULL
keymap "^[[I" NULL

#### Page Down key ####

keymap "^[[6~" NULL
keymap "^[[G" NULL

#### Home key ####

keymap "^[OH" NULL
keymap "^[[1~" NULL
keymap "^[[7~" NULL
keymap "^[[H" NULL

#### End key ####

keymap "^[OF" NULL
keymap "^[[4~" NULL
keymap "^[[8~" NULL
keymap "^[[F" NULL

