	func "5  5%" COMMAND "5" "PRINT /tmp/w3mplus_view_cache; GOTO file:///cgi-bin/w3mplus?action=gotoPercent&percent=5&file=/tmp/w3mplus_view_cache"
	func "6  6%" COMMAND "6" "PRINT /tmp/w3mplus_view_cache; GOTO file:///cgi-bin/w3mplus?action=gotoPercent&percent=6&file=/tmp/w3mplus_view_cache"
	func "7  7%" COMMAND "7" "PRINT /tmp/w3mplus_view_cache; GOTO file:///cgi-bin/w3mplus?action=gotoPercent&percent=7&file=/tmp/w3mplus_view_cache"
	func "8  8%" COMMAND "8" "PRINT /tmp/w3mplus_view_cache; GOTO file:///cgi-bin/w3mplus?action=gotoPercent&percent=8&file=/tmp/w3mplus_view_cache"
