menu operatorFunc
	func  "@,h,l  Current line" COMMAND "@hl" "PRINT /tmp/w3mplus_view_cache; GOTO file:///cgi-bin/w3mplus?action=selectLine&subaction=operatorFunc&number=0&file=/tmp/w3mplus_view_cache"
	func  "j      Down line" COMMAND "j" "PRINT /tmp/w3mplus_view_cache; GOTO file:///cgi-bin/w3mplus?action=selectLine&subaction=operatorFunc&number=%2B1&file=/tmp/w3mplus_view_cache"
	func  "k      Up line" COMMAND "k" "PRINT /tmp/w3mplus_view_cache; GOTO file:///cgi-bin/w3mplus?action=selectLine&subaction=operatorFunc&number=-1&file=/tmp/w3mplus_view_cache"
	func  "g      Top line" COMMAND "g" "PRINT /tmp/w3mplus_view_cache; GOTO file:///cgi-bin/w3mplus?action=selectLine&subaction=operatorFunc&number=1&file=/tmp/w3mplus_view_cache"
	func  "G      Bottom line" COMMAND "G" "PRINT /tmp/w3mplus_view_cache; GOTO file:///cgi-bin/w3mplus?action=selectLine&subaction=operatorFunc&number=$&file=/tmp/w3mplus_view_cache"
	func  "*      All line" COMMAND "*" "PRINT /tmp/w3mplus_view_cache; GOTO file:///cgi-bin/w3mplus?action=selectLine&subaction=operatorFunc&line=1&number=$&file=/tmp/w3mplus_view_cache"
	nop  "--------------------"
	popup "1      1 line" operatorFunc1Line "1"
	popup "2      2 line" operatorFunc2Line "2"
	popup "3      3 line" operatorFunc3Line "3"
	popup "4      4 line" operatorFunc4Line "4"
	popup "5      5 line" operatorFunc5Line "5"
	popup "6      6 line" operatorFunc6Line "6"
	popup "7      7 line" operatorFunc7Line "7"
	popup "8      8 line" operatorFunc8Line "8"
	popup "9      9 line" operatorFunc9Line "9"
end

menu operatorFunc1Line
	func "j  Down one line" COMMAND "j" "PRINT /tmp/w3mplus_view_cache; GOTO file:///cgi-bin/w3mplus?action=selectLine&subaction=operatorFunc&number=%2B1&file=/tmp/w3mplus_view_cache"
	func "k  Up one line" COMMAND "k" "PRINT /tmp/w3mplus_view_cache; GOTO file:///cgi-bin/w3mplus?action=selectLine&subaction=operatorFunc&number=-1&file=/tmp/w3mplus_view_cache"
	func "*  Up line & Down line" COMMAND "*" "PRINT /tmp/w3mplus_view_cache; GOTO file:///cgi-bin/w3mplus?action=selectLine&subaction=operatorFunc&number=%2B-1&file=/tmp/w3mplus_view_cache"
end

