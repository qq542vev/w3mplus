keymap "^[[34~" NULL

### Key used in w3m ###

keymap "^[[E" NULL
keymap "^[[L" NULL
keymap "^[[Z" NULL

# Defining a Lynx like keymap #

keymap "C-@" COMMAND "SET_OPTION keymap_file=keymap_w3mplus; MARK"
keymap "C-a" COMMAND "SET_OPTION keymap_file=keymap_w3mplus; BEGIN"
keymap "C-b" COMMAND "SET_OPTION keymap_file=keymap_w3mplus; BACK"
keymap "C-e" COMMAND "SET_OPTION keymap_file=keymap_w3mplus; END"
keymap "C-f" COMMAND "SET_OPTION keymap_file=keymap_w3mplus; GOTO_LINK"
keymap "C-g" COMMAND "SET_OPTION keymap_file=keymap_w3mplus; LINE_INFO"
keymap "C-h" COMMAND "SET_OPTION keymap_file=keymap_w3mplus; HISTORY"
keymap "TAB" COMMAND "SET_OPTION keymap_file=keymap_w3mplus; NEXT_LINK"
keymap "C-j" COMMAND "SET_OPTION keymap_file=keymap_w3mplus; GOTO_LINK"
keymap "C-k" COMMAND "SET_OPTION keymap_file=keymap_w3mplus; COOKIE"
keymap "C-l" COMMAND "SET_OPTION keymap_file=keymap_w3mplus; REDRAW"
keymap "C-m" COMMAND "SET_OPTION keymap_file=keymap_w3mplus; GOTO_LINK"
keymap "C-n" COMMAND "SET_OPTION keymap_file=keymap_w3mplus; NEXT_LINK"
keymap "C-p" COMMAND "SET_OPTION keymap_file=keymap_w3mplus; PREV_LINK"
keymap "C-q" COMMAND "SET_OPTION keymap_file=keymap_w3mplus; CLOSE_TAB"
keymap "C-r" COMMAND "SET_OPTION keymap_file=keymap_w3mplus; RELOAD"
keymap "C-s" COMMAND "SET_OPTION keymap_file=keymap_w3mplus; SEARCH"
keymap "C-t" COMMAND "SET_OPTION keymap_file=keymap_w3mplus; TAB_LINK"
keymap "C-u" COMMAND "SET_OPTION keymap_file=keymap_w3mplus; PREV_LINK"
keymap "C-v" COMMAND "SET_OPTION keymap_file=keymap_w3mplus; NEXT_PAGE"
keymap "C-w" COMMAND "SET_OPTION keymap_file=keymap_w3mplus; REDRAW"
keymap "C-z" COMMAND "SET_OPTION keymap_file=keymap_w3mplus; SUSPEND"

keymap "SPC" COMMAND "SET_OPTION keymap_file=keymap_w3mplus; NEXT_PAGE"
keymap "!" COMMAND "SET_OPTION keymap_file=keymap_w3mplus; SHELL"
keymap "\"" COMMAND "SET_OPTION keymap_file=keymap_w3mplus; REG_MARK"
keymap "#" COMMAND "SET_OPTION keymap_file=keymap_w3mplus; PIPE_SHELL"
keymap "$" COMMAND "SET_OPTION keymap_file=keymap_w3mplus; LINE_END"
keymap "(" COMMAND "SET_OPTION keymap_file=keymap_w3mplus; UNDO"
keymap ")" COMMAND "SET_OPTION keymap_file=keymap_w3mplus; REDO"
keymap "+" COMMAND "SET_OPTION keymap_file=keymap_w3mplus; NEXT_PAGE"
keymap "," COMMAND "SET_OPTION keymap_file=keymap_w3mplus; LEFT"
keymap "-" COMMAND "SET_OPTION keymap_file=keymap_w3mplus; PREV_PAGE"
keymap "." COMMAND "SET_OPTION keymap_file=keymap_w3mplus; RIGHT"
keymap "/" COMMAND "SET_OPTION keymap_file=keymap_w3mplus; SEARCH"
keymap ":" COMMAND "SET_OPTION keymap_file=keymap_w3mplus; MARK_URL"
keymap ";" COMMAND "SET_OPTION keymap_file=keymap_w3mplus; MARK_WORD"
keymap "<" COMMAND "SET_OPTION keymap_file=keymap_w3mplus; SHIFT_LEFT"
keymap "=" COMMAND "SET_OPTION keymap_file=keymap_w3mplus; INFO"
keymap ">" COMMAND "SET_OPTION keymap_file=keymap_w3mplus; SHIFT_RIGHT"
keymap "?" COMMAND "SET_OPTION keymap_file=keymap_w3mplus; HELP"
keymap "@" COMMAND "SET_OPTION keymap_file=keymap_w3mplus; READ_SHELL"
keymap "B" COMMAND "SET_OPTION keymap_file=keymap_w3mplus; BACK"
keymap "D" COMMAND "SET_OPTION keymap_file=keymap_w3mplus; DOWNLOAD_LIST"
keymap "E" COMMAND "SET_OPTION keymap_file=keymap_w3mplus; EDIT"
keymap "F" COMMAND "SET_OPTION keymap_file=keymap_w3mplus; FRAME"
keymap "G" COMMAND "SET_OPTION keymap_file=keymap_w3mplus; GOTO_LINE"
keymap "H" COMMAND "SET_OPTION keymap_file=keymap_w3mplus; HELP"
keymap "I" COMMAND "SET_OPTION keymap_file=keymap_w3mplus; VIEW_IMAGE"
keymap "J" COMMAND "SET_OPTION keymap_file=keymap_w3mplus; UP"
keymap "K" COMMAND "SET_OPTION keymap_file=keymap_w3mplus; DOWN"
keymap "L" COMMAND "SET_OPTION keymap_file=keymap_w3mplus; LIST"
keymap "M" COMMAND "SET_OPTION keymap_file=keymap_w3mplus; EXTERN"
keymap "N" COMMAND "SET_OPTION keymap_file=keymap_w3mplus; NEXT_MARK"
keymap "P" COMMAND "SET_OPTION keymap_file=keymap_w3mplus; PREV_MARK"
keymap "Q" COMMAND "SET_OPTION keymap_file=keymap_w3mplus; EXIT"
keymap "R" COMMAND "SET_OPTION keymap_file=keymap_w3mplus; RELOAD"
keymap "S" COMMAND "SET_OPTION keymap_file=keymap_w3mplus; SAVE_SCREEN"
keymap "T" COMMAND "SET_OPTION keymap_file=keymap_w3mplus; NEW_TAB"
keymap "U" COMMAND "SET_OPTION keymap_file=keymap_w3mplus; GOTO"
keymap "V" COMMAND "SET_OPTION keymap_file=keymap_w3mplus; LOAD"
keymap "W" COMMAND "SET_OPTION keymap_file=keymap_w3mplus; PREV_WORD"
keymap "Z" COMMAND "SET_OPTION keymap_file=keymap_w3mplus; CENTER_H"
keymap "[" COMMAND "SET_OPTION keymap_file=keymap_w3mplus; LINK_BEGIN"
keymap "\\" COMMAND "SET_OPTION keymap_file=keymap_w3mplus; SOURCE"
keymap "]" COMMAND "SET_OPTION keymap_file=keymap_w3mplus; LINK_END"
keymap "\^" COMMAND "SET_OPTION keymap_file=keymap_w3mplus; LINE_BEGIN"
keymap "a" COMMAND "SET_OPTION keymap_file=keymap_w3mplus; ADD_BOOKMARK"
keymap "b" COMMAND "SET_OPTION keymap_file=keymap_w3mplus; PREV_PAGE"
keymap "c" COMMAND "SET_OPTION keymap_file=keymap_w3mplus; PEEK"
keymap "d" COMMAND "SET_OPTION keymap_file=keymap_w3mplus; SAVE_LINK"
keymap "g" COMMAND "SET_OPTION keymap_file=keymap_w3mplus; GOTO"
keymap "h" COMMAND "SET_OPTION keymap_file=keymap_w3mplus; MOVE_LEFT"
keymap "i" COMMAND "SET_OPTION keymap_file=keymap_w3mplus; PEEK_IMG"
keymap "j" COMMAND "SET_OPTION keymap_file=keymap_w3mplus; MOVE_DOWN"
keymap "k" COMMAND "SET_OPTION keymap_file=keymap_w3mplus; MOVE_UP"
keymap "l" COMMAND "SET_OPTION keymap_file=keymap_w3mplus; MOVE_RIGHT"
keymap "m" COMMAND "SET_OPTION keymap_file=keymap_w3mplus; MOUSE_TOGGLE"
keymap "n" COMMAND "SET_OPTION keymap_file=keymap_w3mplus; SEARCH_NEXT"
keymap "o" COMMAND "SET_OPTION keymap_file=keymap_w3mplus; OPTIONS"
keymap "p" COMMAND "SET_OPTION keymap_file=keymap_w3mplus; SAVE_SCREEN"
keymap "q" COMMAND "SET_OPTION keymap_file=keymap_w3mplus; QUIT"
keymap "r" COMMAND "SET_OPTION keymap_file=keymap_w3mplus; VERSION"
keymap "s" COMMAND "SET_OPTION keymap_file=keymap_w3mplus; SELECT_MENU"
keymap "u" COMMAND "SET_OPTION keymap_file=keymap_w3mplus; PEEK_LINK"
keymap "v" COMMAND "SET_OPTION keymap_file=keymap_w3mplus; BOOKMARK"
keymap "w" COMMAND "SET_OPTION keymap_file=keymap_w3mplus; NEXT_WORD"
keymap "z" COMMAND "SET_OPTION keymap_file=keymap_w3mplus; CENTER_V"
keymap "{" COMMAND "SET_OPTION keymap_file=keymap_w3mplus; PREV_TAB"
keymap "|" COMMAND "SET_OPTION keymap_file=keymap_w3mplus; PIPE_BUF"
keymap "}" COMMAND "SET_OPTION keymap_file=keymap_w3mplus; NEXT_TAB"

keymap "M-TAB" COMMAND "SET_OPTION keymap_file=keymap_w3mplus; PREV_LINK"
keymap "M-C-j" COMMAND "SET_OPTION keymap_file=keymap_w3mplus; SAVE_LINK"
keymap "M-C-m" COMMAND "SET_OPTION keymap_file=keymap_w3mplus; SAVE_LINK"

keymap "M-:" COMMAND "SET_OPTION keymap_file=keymap_w3mplus; MARK_MID"
keymap "M-<" COMMAND "SET_OPTION keymap_file=keymap_w3mplus; BEGIN"
keymap "M->" COMMAND "SET_OPTION keymap_file=keymap_w3mplus; END"
keymap "M-I" COMMAND "SET_OPTION keymap_file=keymap_w3mplus; SAVE_IMAGE"
keymap "M-M" COMMAND "SET_OPTION keymap_file=keymap_w3mplus; EXTERN_LINK"
keymap "M-W" COMMAND "SET_OPTION keymap_file=keymap_w3mplus; DICT_WORD_AT"
keymap "M-a" COMMAND "SET_OPTION keymap_file=keymap_w3mplus; ADD_BOOKMARK"
keymap "M-b" COMMAND "SET_OPTION keymap_file=keymap_w3mplus; BOOKMARK"
keymap "M-c" COMMAND "SET_OPTION keymap_file=keymap_w3mplus; COMMAND"
keymap "M-e" COMMAND "SET_OPTION keymap_file=keymap_w3mplus; EDIT_SCREEN"
keymap "M-g" COMMAND "SET_OPTION keymap_file=keymap_w3mplus; GOTO_LINE"
keymap "M-k" COMMAND "SET_OPTION keymap_file=keymap_w3mplus; DEFINE_KEY"
keymap "M-l" COMMAND "SET_OPTION keymap_file=keymap_w3mplus; LIST_MENU"
keymap "M-m" COMMAND "SET_OPTION keymap_file=keymap_w3mplus; MOVE_LIST_MENU"
keymap "M-n" COMMAND "SET_OPTION keymap_file=keymap_w3mplus; NEXT_MARK"
keymap "M-o" COMMAND "SET_OPTION keymap_file=keymap_w3mplus; SET_OPTION"
keymap "M-p" COMMAND "SET_OPTION keymap_file=keymap_w3mplus; PREV_MARK"
keymap "M-s" COMMAND "SET_OPTION keymap_file=keymap_w3mplus; SAVE"
keymap "M-t" COMMAND "SET_OPTION keymap_file=keymap_w3mplus; TAB_MENU"
keymap "M-u" COMMAND "SET_OPTION keymap_file=keymap_w3mplus; GOTO_RELATIVE"
keymap "M-v" COMMAND "SET_OPTION keymap_file=keymap_w3mplus; PREV_PAGE"
keymap "M-w" COMMAND "SET_OPTION keymap_file=keymap_w3mplus; DICT_WORD"

keymap "UP" COMMAND "SET_OPTION keymap_file=keymap_w3mplus; PREV_LINK"
keymap "DOWN" COMMAND "SET_OPTION keymap_file=keymap_w3mplus; NEXT_LINK"
keymap "RIGHT" COMMAND "SET_OPTION keymap_file=keymap_w3mplus; GOTO_LINK"
keymap "LEFT" COMMAND "SET_OPTION keymap_file=keymap_w3mplus; BACK"

keymap "M-[E" COMMAND "SET_OPTION keymap_file=keymap_w3mplus; MENU"
keymap "M-[L" COMMAND "SET_OPTION keymap_file=keymap_w3mplus; MENU"

keymap "M-[1~" COMMAND "SET_OPTION keymap_file=keymap_w3mplus; BEGIN"
keymap "M-[2~" COMMAND "SET_OPTION keymap_file=keymap_w3mplus; MENU"
keymap "M-[4~" COMMAND "SET_OPTION keymap_file=keymap_w3mplus; END"
keymap "M-[5~" COMMAND "SET_OPTION keymap_file=keymap_w3mplus; PREV_PAGE"
keymap "M-[6~" COMMAND "SET_OPTION keymap_file=keymap_w3mplus; NEXT_PAGE"
keymap "M-[28~" COMMAND "SET_OPTION keymap_file=keymap_w3mplus; MENU"
