menu filter
	func  "!,h,l  現在の行" GOTO "!hl" "file:///cgi-bin/w3mplus?action=selectLine&subaction=filter&number=0"
	func  "j      下の1行" GOTO "j" "file:///cgi-bin/w3mplus?action=selectLine&subaction=filter&number=%2B1"
	func  "k      上の1行" GOTO "k" "file:///cgi-bin/w3mplus?action=selectLine&subaction=filter&number=-1"
	func  "g      すべての上の行" GOTO "g" "file:///cgi-bin/w3mplus?action=selectLine&subaction=filter&number=1"
	func  "G      すべての下の行" GOTO "G" "file:///cgi-bin/w3mplus?action=selectLine&subaction=filter&number=$"
	func  "*      すべての行" PIPE_BUF "*"
	nop   "---------------------"
	popup "1      1行" filter1Line "1"
	popup "2      2行" filter2Line "2"
	popup "3      3行" filter3Line "3"
	popup "4      4行" filter4Line "4"
	popup "5      5行" filter5Line "5"
	popup "6      6行" filter6Line "6"
	popup "7      7行" filter7Line "7"
	popup "8      8行" filter8Line "8"
	popup "9      9行" filter9Line "9"
end

