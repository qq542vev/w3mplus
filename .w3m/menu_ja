# menu MENU_ID
#  func  LABEL FUNCTION KEYS
#  popup LABEL MENU_ID  KEYS
#  nop   LABEL
# end
#
# MENU_ID
#  Main:   Main menu
#  Select: Buffer selection menu

menu Main
	func "戻る                        (b)" PREV "Bb"
	func "進む                        (f)" NEXT "Ff"
	func "更新                        (r)" RELOAD "Rr"
	nop  "-------------------------------"
	func "このページをブックマーク    (m)" ADD_BOOKMARK "Mm"
	func "名前を付けてページを保存... (p)" SAVE "Pp"
	func "ページ URL を送信...        (e)" GOTO "Ee" "file:///cgi-bin/w3mplus?action=uri&subaction=sendEmail"
	nop  "-------------------------------"
	func "ページのソースを表示        (v)" VIEW "Vv"
	func "ページの情報を表示          (i)" INFO "Ii"
end

menu ContextMenuLink
	func "リンクを新しいタブで開く                   (t)" TAB_LINK "Tt"
	func "リンクを新しいウィンドウで開く             (w)" EXTERN_LINK "Ww" "w3m %s"
	func "リンクを新しいプライベートウィンドウで開く (p)" EXTERN_LINK "Pp" "w3m-private %s"
	func "選択した URL を開く                        (o)" GOTO_LINK "Oo"
	nop  "----------------------------------------------"
	func "このリンクをブックマーク                   (l)" GOTO "Ll" "file:///cgi-bin/w3mplus?action=uri&subaction=addBookmark&uri=%24%7BW3M_CURRENT_LINK%7D"
	func "名前を付けてリンク先を保存...              (k)" SAVE_LINK "Kk"
	func "リンクを送信...                            (d)" GOTO "Dd" "file:///cgi-bin/w3mplus?action=uri&subaction=sendEmail&uri=%24%7BW3M_CURRENT_LINK%7D"
	func "リンクの URL をコピー                      (a)" GOTO "Aa" "file:///cgi-bin/w3mplus?action=yank&yank=%24%7BW3M_CURRENT_LINK%7D"
end

menu ContextMenuImage
	func "画像だけを表示               (i)" VIEW_IMAGE "Ii"
	func "画像の URL をコピー          (o)" GOTO "Oo" "file:///cgi-bin/w3mplus?action=yank&yank=%24%7BW3M_CURRENT_IMG%7D"
	nop  "--------------------------------"
	func "名前を付けて画像を保存...    (v)" SAVE_IMAGE "Vv"
	func "画像の URL をメールで送信... (g)" GOTO "Gg" "file:///cgi-bin/w3mplus?action=uri&subaction=sendEmail&uri=%24%7BW3M_CURRENT_IMG%7D"
end

menu ContextMenuLinkImage
	func "リンクを新しいタブで開く           (t)" TAB_LINK "Tt"
	func "リンクを新しいウィンドウで開く     (w)" EXTERN_LINK "Ww" "w3m %s"
	func "新しいプライベートウィンドウで開く (p)" EXTERN_LINK "Pp" "w3m-private %s"
	func "選択した URL をコピー              (O)" GOTO_LINK "O"
	nop  "--------------------------------------"
	func "このリンクをブックマーク           (l)" GOTO "Ll" "file:///cgi-bin/w3mplus?action=uri&subaction=addBookmark&uri=%24%7BW3M_CURRENT_LINK%7D"
	func "名前を付けてリンク先を保存...      (k)" SAVE_LINK "Kk"
	func "リンクを送信...                    (d)" GOTO "Dd" "file:///cgi-bin/w3mplus?action=uri&subaction=sendEmail&uri=%24%7BW3M_CURRENT_LINK%7D"
	func "リンクの URL をコピー              (a)" GOTO "Aa" "file:///cgi-bin/w3mplus?action=yank&yank=%24%7BW3M_CURRENT_LINK%7D"
	nop  "--------------------------------------"
	func "画像だけを表示                     (i)" VIEW_IMAGE "Ii"
	func "画像の URL をコピー                (o)" GOTO "Oo" "file:///cgi-bin/w3mplus?action=yank&yank=%24%7BW3M_CURRENT_IMG%7D"
	nop  "--------------------------------------"
	func "名前を付けて画像を保存...          (v)" SAVE_IMAGE "Vv"
	func "画像の URL をメールで送信...       (g)" GOTO "Gg" "file:///cgi-bin/w3mplus?action=uri&subaction=sendEmail&uri=%24%7BW3M_CURRENT_IMG%7D"
end

menu ContextMenuTab
	func "新しいタブ             (n)" NEW_TAB "Nn"
	func "タブを再読み込み       (t)" RELOAD "Tt"
	func "新しいウィンドウへ移動 (w)" EXTERN "Ww" "w3m %s"
	nop  "--------------------------"
	func "閉じたタブを元に戻す   (u)" GOTO "u" "file:///cgi-bin/w3mplus?action=undoTab"
	func "タブを閉じる           (c)" COMMAND "Cc" "TAB_RIGHT; GOTO file:///cgi-bin/w3mplus?action=closeTab"
	func "他のタブをすべて閉じる (o)" COMMAND "Oo" "TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; PREV_TAB; CLOSE_TAB; CLOSE_TAB; CLOSE_TAB; CLOSE_TAB; CLOSE_TAB; CLOSE_TAB; CLOSE_TAB; CLOSE_TAB; CLOSE_TAB; CLOSE_TAB; CLOSE_TAB; CLOSE_TAB; CLOSE_TAB; CLOSE_TAB; CLOSE_TAB; CLOSE_TAB; CLOSE_TAB; CLOSE_TAB; CLOSE_TAB; CLOSE_TAB"
	nop  "--------------------------"
	func "タブを左に移動         (l)" TAB_LEFT "l"
	func "タブを右に移動         (r)" TAB_RIGHT "r"
	nop  "--------------------------"
	func "タブを最も左に移動     (L)" COMMAND "L" "TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT"
	func "タブを最も右に移動     (R)" COMMAND "R" "TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT"
	nop  "--------------------------"
	func "すべてのタブを表示     (a)" TAB_MENU "Aa"
end

menu Menu
	popup "ファイル     (f)" MenuFile "Ff"
	popup "編集         (e)" MenuEdit "Ee"
	popup "表示         (v)" MenuView "Vv"
	popup "履歴         (s)" MenuHistory "Ss"
	popup "ブックマーク (b)" MenuBookmarks "Bb"
	popup "ツール       (t)" MenuTools "Tt"
	popup "ヘルプ       (h)" MenuHelp "Hh"
end

menu MenuFile
	func "新しいタブ                     (t)" NEW_TAB "Tt"
	func "新しいウィンドウ               (n)" EXTERN "Nn" "w3m ${WWW_HOME-about:home} #"
	func "新しいプライベートウィンドウ   (w)" EXTERN "Ww" "w3m-private about:home #"
	func "ファイルを開く...              (o)" LOAD "Oo"
	nop  "----------------------------------"
	func "名前を付けてページを保存...    (a)" DOWNLOAD "Aa"
	func "ページの URL をメールで送信... (e)" GOTO "Ee" "file:///cgi-bin/w3mplus?action=uri&subaction=sendEmail&uri=%24%7BW3M_URL%7D"
	nop  "----------------------------------"
	func "印刷                           (p)" PRINT "Pp"
	nop  "----------------------------------"
	func "設定の再読み込み               (r)" REINIT "Rr"
	func "中断                           (s)" SUSPEND "Ss"
	func "終了                           (q)" EXIT "Qq"
end

menu MenuEdit
	func "元に戻す   (u)" UNDO "Uu"
	func "やり直し   (r)" REDO "Rr"
	nop  "--------------"
	func "コピー     (c)" GOTO "Cc" "file:///cgi-bin/w3mplus?action=yank&yank=%24%7BW3M_CURRENT_WORD%7D"
	func "マーク     (m)" MARK "Mm"
	nop  "--------------"
	func "検索       (f)" ISEARCH "Ff"
	func "ハイライト (h)" REG_MARK "Hh"
end

menu MenuView
	popup "ツールバー           (t)" MenuViewToolbars "Tt"
	popup "サイドバー           (e)" MenuViewSidebar "Ee"
	nop   "------------------------"
	popup "ズーム               (z)" MenuViewZoom "Zz"
	func  "文字エンコーディング (c)" CHARSET "Cc"
	func  "ナビゲーションリンク (n)" LINK_MENU "Nn"
	func  "すべてのリンクを表示 (l)" LIST_MENU "Ll"
	nop   "------------------------"
	func  "すべてのタブを表示   (a)" TAB_MENU "Aa"
end

menu MenuViewToolbars
	func "ブックマークツールバー (b)" GOTO "Bb" "file:///cgi-bin/w3mplus?action=bookmarkBar&type=topToolbar"
end

menu MenuViewSidebar
	func "ブックマーク (b)" GOTO "Bb" "file:///cgi-bin/w3mplus?action=bookmarkBar"
end

menu MenuViewZoom
	func "拡大     (i)" GOTO "Ii" "file:///cgi-bin/w3mplus?action=zoom&zoom=%2B20"
	func "縮小     (o)" GOTO "Oo" "file:///cgi-bin/w3mplus?action=zoom&zoom=-20"
	nop  "------------"
	func "リセット (r)" GOTO "Rr" "file:///cgi-bin/w3mplus?action=zoom&zoom=100"
end

menu MenuHistory
	func  "すべての履歴を表示 (h)" HISTORY "Hh"
	func  "タブの履歴を表示   (b)" SELECT_MENU "Bb"
	popup "最近閉じたタブ     (u)" MenuUndoClosedTabs "Uu"
end

menu MenuUndoClosedTabs
	func "1個のタブを復元      (u)" GOTO "Uu" "file:///cgi-bin/w3mplus?action=undoTab"
	func "5個のタブを復元      (f)" GOTO "Ff" "file:///cgi-bin/w3mplus?action=undoTab&count=-5"
	func "10個のタブを復元     (t)" GOTO "Tt" "file:///cgi-bin/w3mplus?action=undoTab&count=-10"
	nop  "------------------------"
	func "10分以内のタブを復元 (1)" GOTO "1" "file:///cgi-bin/w3mplus?action=undoTab&count=%40-600"
	func "30分以内のタブを復元 (3)" GOTO "3" "file:///cgi-bin/w3mplus?action=undoTab&count=%40-1800"
	func "60分以内のタブを復元 (6)" GOTO "6" "file:///cgi-bin/w3mplus?action=undoTab&count=%40-3600"
	nop  "------------------------"
	func "すべてのタブを復元   (a)" GOTO "Aa" "file:///cgi-bin/w3mplus?action=undoTab&count=-0"
end

menu MenuBookmarks
	func "このページをブックマーク (m)" ADD_BOOKMARK "Mm"
	func "ブックマークを表示       (v)" VIEW_BOOKMARK "Vv"
end

menu MenuTools
	func  "ダウンロード     (d)" DOWNLOAD_LIST "Dd"
	func  "Cookies          (c)" COOKIE "Cc"
	func  "サイト別設定     (m)" GOTO "Mm" "about:permissions"
	nop   "--------------------"
	func  "ページのソース   (o)" SOURCE "Oo"
	func  "エラーメッセージ (e)" MSGS "Ee"
	func  "ページの情報     (i)" INFO "Ii"
	nop   "--------------------"
	popup "Cookie の管理    (k)" MenuCookieManager "Kk"
	popup "User Agent       (u)" MenuUserAgent "Uu"
	func  "設定             (p)" OPTIONS "Pp"
end

menu MenuCookieManager
	nop  "-- ドメインをブラックリストに適用 --"
	func "サブドメインも適用               (b)" GOTO "b" "file:///cgi-bin/w3mplus?action=cookieManager&blacklist=toggle&subdomain=1"
	func "ページのドメインのみ適用         (B)" GOTO "B" "file:///cgi-bin/w3mplus?action=cookieManager&blacklist=toggle"
	nop  "-- ドメインをホワイトリストに適用 --"
	func "サブドメインも適用               (w)" GOTO "w" "file:///cgi-bin/w3mplus?action=cookieManager&whitelist=toggle&subdomain=1"
	func "ページのドメインのみ適用         (W)" GOTO "W" "file:///cgi-bin/w3mplus?action=cookieManager&whitelist=toggle
end

menu MenuUserAgent
	nop  "-- CLI Web ブラウザー ----"
	func "w3m                    (3)" SET_OPTION "3" "user_agent="
	func "lynx                   (l)" SET_OPTION "l" "user_agent=Lynx/2.8.9rel.1 libwww-FM/2.14 SSL-MM/1.4.1 GNUTLS/3.6.5"
	nop  "-- GUI Web ブラウザー ----"
	func "Internet Explorer      (i)" SET_OPTION "i" "user_agent=Mozilla/5.0 (Windows NT 6.3; Win64; x64; Trident/7.0; rv:11.0) like Gecko"
	func "Microsoft Edge         (e)" SET_OPTION "e" "user_agent=Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/70.0.35g38.102 Safari/537.36 Edge/18.18363"
	func "Safari                 (s)" SET_OPTION "s" "user_agent=Mozilla/5.0 (Macintosh; Intel Mac OS X 10_9_3) AppleWebKit/537.75.14 (KHTML, like Gecko) Version/7.0.3 Safari/7046A194A"
	func "Google Chrome          (c)" SET_OPTION "c" "user_agent=Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/79.0.3945.79 Safari/537.36"
	func "Mozilla Firefox        (f)" SET_OPTION "f" "user_agent=Mozilla/5.0 (Windows NT 10.0; Win64; x64; rv:68.0) Gecko/20100101 Firefox/68.0"
	func "PaleMoon               (p)" SET_OPTION "p" "user_agent=Mozilla/5.0 (Windows NT 10.0; Win64; x64; rv:68.9) Goanna/4.4 PaleMoon/28.8.0"
	func "NetSurf                (n)" SET_OPTION "n" "user_agent=NetSurf/3.6 (Linux; x86_64)"
	nop  "-- モバイル Web ブラウザー"
	func "android (Chrome)       (a)" SET_OPTION "a" "user_agent=Mozilla/5.0 (Linux; U; Android 6.0) AppleWebKit/537.16 (KHTML, like Gecko) Version/4.0 Mobile Safari/537.16 Chrome/33.0.0.0"
	func "iPhone  (Safari)       (i)" SET_OPTION "i" "user_agent=Mozilla/5.0 (iPhone; CPU iPhone OS 13_2_3 like Mac OS X) AppleWebKit/605.1.15 (KHTML, like Gecko) Version/13.0.3 Mobile/15E148 Safari/604.1"
	func "Firefox for Mobile     (F)" SET_OPTION "F" "user_agent=Mozilla/5.0 (Android 6.0.1; Mobile; rv:68.0) Gecko/68.0 Firefox/68.0"
	func "Windows 10 Mobile      (w)" SET_OPTION "w" "user_agent=Mozilla/5.0 (Windows Phone 10.0; Android 6.0.1; Microsoft; Lumia 950 XL) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/51.0.2704.79 Mobile Safari/537.36 Edge/14.14393"
	nop  "-- Web クローラー --------"
	func "Googlebot              (g)" SET_OPTION "g" "user_agent=Mozilla/5.0 (compatible; Googlebot/2.1; +http://www.google.com/bot.html)"
	func "Bingbot                (b)" SET_OPTION "b" "user_agent=Mozilla/5.0 (compatible; bingbot/2.0; +http://www.bing.com/bingbot.htm)"
	func "Baiduspider            (B)" SET_OPTION "B" "user_agent=Mozilla/5.0 (compatible; Baiduspider/2.0; +http://www.baidu.com/search/spider.html)"
	nop  "-- ダウンロードツール ----"
	func "Wget                   (W)" SET_OPTION "W" "user_agent=Wget/1.20.1 (linux-gnueabihf)"
	func "curl                   (C)" SET_OPTION "C" "user_agent=curl/7.64.0"
end

menu MenuHelp
	func "w3m ヘルプ   (h)" HELP "Hh"
	func "Man w3m      (m)" EXEC_SHELL "Mm" "man w3m"
	nop  "----------------"
	func "w3m について (a)" VERSION "Aa"
end

menu ExtendedHints
	func "?  行の情報を表示" LINE_INFO "?"
	func "s  リンク先を保存" SAVE_LINK "s"
	func "o  リンクを開く" GOTO_LINK "o"
	func "t  リンクを新しいタブで開く" TAB_LINK "t"
	func "b  バックグラウンドタブでリンクを開く" TAB_GOTO "b" "file:///cgi-bin/w3mplus?action=uri&subaction=prevTab&uri=%24%7BW3M_CURRENT_LINK%7D&redirect=2"
	func "w  リンクを新しいウィンドウで開く" EXTERN_LINK "w" "w3m %s"
	func "O  Generate a ':o URL' prompt" GOTO "O" "file:///cgi-bin/w3mplus?action=linkSetOpen"
	func "T  Generate a ':t URL' prompt" GOTO "T" "file:///cgi-bin/w3mplus?action=linkSetTabopen"
	func "W  リンクを外部のブラウザーで開く" EXTERN_LINK "W"
	func "a  ブックマークに追加" GOTO "a" "file:///cgi-bin/w3mplus?action=uri&subaction=addBookmark&uri=%24%7BW3M_CURRENT_LINK%7D"
	func "v  リンク先のソースを表示" GOTO "v" "file:///cgi-bin/w3mplus?action=uri&subaction=viewSource&uri=%24%7BW3M_CURRENT_LINK%7D"
	func "V  リンク先のソースを外部エディターで表示" GOTO "V" "file:///cgi-bin/w3mplus?action=uri&subaction=viewSourceExternally&uri=%24%7BW3M_CURRENT_LINK%7D"
	func "y  リンクの URL をヤンク" GOTO "y" "file:///cgi-bin/w3mplus?action=yank&yank=%24%7BW3M_CURRENT_LINK%7D"
	func "Y  リンクテキストをヤンク" GOTO "Y" "file:///cgi-bin/w3mplus?action=yank&yank=%24%7BW3M_CURRENT_WORD%7D"
	func "c  コンテキストメニューを開く" MENU "c"
	func "i  画像を表示" VIEW_IMAGE "i"
	func "I  画像を保存" SAVE_IMAGE "I"
end

menu Commands
	func "o  現在のタブで URL を開く" COMMAND "o" "SET_OPTION default_url=0; GOTO"
	func "t  新しいタブで URL を開く" COMMAND "t" "SET_OPTION default_url=0; TAB_GOTO"
	func "j  ジャンプリストを表示" SELECT_MENU "j"
	func "w  現在のページをディスクに保存" DOWNLOAD "w"
	func "W  現在のページを印刷" PRINT "W"
	func "x  w3m を終了" EXIT "x"
	func "q  w3m を終了" QUIT "q"
	func "P  カレントディレクトリ名を表示" EXEC_SHELL "P" pwd
	func "f  HTML フレームの描写の切り替え" FRAME "f"
	func "b  ブックマークを開く" VIEW_BOOKMARK "b"
	func "H  最近訪れた URL を表示" HISTORY "H"
	func "e  w3m メニューを表示" MENU "e" Menu
	func "d  ダウンロードリストを表示" DOWNLOAD_LIST "d"
	func "v  バージョン情報を表示" VERSION "v"
	func "m  以前のメッセージを表示" MSGS "m"
	func "s  ローカルオプションを設定" SET_OPTION "s"
	func "S  グローバルオプションを設定" SET_ENV "S"
	func "!  コマンドを実行" EXEC_SHELL "!"
	func "p  w3m の設定を表示" OPTIONS "p"
	func "M  キー入力を設定" DEFINE_KEY "M"
	func "r  シェルコマンドを実行して出力を表示" PIPE_SHELL "r"
	func ":  w3m のコマンドを実行" COMMAND ":"
	func "h  入門用のヘルプを開く" HELP "h"
	func "c  サイトのCookieアクセス許可を変更" COOKIE "c"
	func "B  ブラウザーの履歴から30ページ戻る" COMMAND "B" "PREV; PREV; PREV; PREV; PREV; PREV; PREV; PREV; PREV; PREV; PREV; PREV; PREV; PREV; PREV; PREV; PREV; PREV; PREV; PREV; PREV; PREV; PREV; PREV; PREV; PREV; PREV; PREV; PREV"
	func "F  ブラウザーの履歴から30ページ進む" COMMAND "F" "NEXT; NEXT; NEXT; NEXT; NEXT; NEXT; NEXT; NEXT; NEXT; NEXT; NEXT; NEXT; NEXT; NEXT; NEXT; NEXT; NEXT; NEXT; NEXT; NEXT; NEXT; NEXT; NEXT; NEXT; NEXT; NEXT; NEXT; NEXT; NEXT"
	func "D  指定されたURLパターンに一致するオートコマンドを適用" GOTO "D" "file:///cgi-bin/w3mplus?action=autoCommand&call=manual"
	func "Q  すべてのQuickMarkを表示する" GOTO "Q" "file:///cgi-bin/w3mplus?action=qmarks"
end

menu Percents
	popup "0   0% -  9%" 0Percents "0"
	popup "1  10% - 19%" 10Percents "1"
	popup "2  20% - 29%" 20Percents "2"
	popup "3  30% - 39%" 30Percents "3"
	popup "4  40% - 49%" 40Percents "4"
	popup "5  50% - 59%" 50Percents "5"
	popup "6  60% - 69%" 60Percents "6"
	popup "7  70% - 79%" 70Percents "7"
	popup "8  80% - 89%" 80Percents "8"
	popup "9  90% - 99%" 90Percents "9"
	func  "$  100%" END "$"
	popup "+  +n% 移動"  plusPercents "+"
	popup "-  -n% 移動"  minusPercents "-"
end

menu 0Percents
	func "0  0%" BEGIN "0"
	func "1  1%" GOTO "1" "file:///cgi-bin/w3mplus?action=movePercent&percent=1"
	func "2  2%" GOTO "2" "file:///cgi-bin/w3mplus?action=movePercent&percent=2"
	func "3  3%" GOTO "3" "file:///cgi-bin/w3mplus?action=movePercent&percent=3"
	func "4  4%" GOTO "4" "file:///cgi-bin/w3mplus?action=movePercent&percent=4"
	func "5  5%" GOTO "5" "file:///cgi-bin/w3mplus?action=movePercent&percent=5"
	func "6  6%" GOTO "6" "file:///cgi-bin/w3mplus?action=movePercent&percent=6"
	func "7  7%" GOTO "7" "file:///cgi-bin/w3mplus?action=movePercent&percent=7"
	func "8  8%" GOTO "8" "file:///cgi-bin/w3mplus?action=movePercent&percent=8"
	func "9  9%" GOTO "9" "file:///cgi-bin/w3mplus?action=movePercent&percent=9"
end

menu 10Percents
	func "0  10%" GOTO "0" "file:///cgi-bin/w3mplus?action=movePercent&percent=10"
	func "1  11%" GOTO "1" "file:///cgi-bin/w3mplus?action=movePercent&percent=11"
	func "2  12%" GOTO "2" "file:///cgi-bin/w3mplus?action=movePercent&percent=12"
	func "3  13%" GOTO "3" "file:///cgi-bin/w3mplus?action=movePercent&percent=13"
