menu switchcase
	func  "~,h,l  現在の行" GOTO "~hl" "file:///cgi-bin/w3mplus?action=selectLine&subaction=switchcase&number=0"
	func  "j      下の1行" GOTO "j" "file:///cgi-bin/w3mplus?action=selectLine&subaction=switchcase&number=%2B1"
	func  "k      上の1行" GOTO "k" "file:///cgi-bin/w3mplus?action=selectLine&subaction=switchcase&number=-1"
	func  "g      すべての上の行" GOTO "g" "file:///cgi-bin/w3mplus?action=selectLine&subaction=switchcase&number=1"
	func  "G      すべての下の行" GOTO "G" "file:///cgi-bin/w3mplus?action=selectLine&subaction=switchcase&number=$"
	func  "*      すべての行" GOTO "*" "file:///cgi-bin/w3mplus?action=selectLine&subaction=switchcase&line=1&number=$"
	nop   "---------------------"
	popup "1      1行" switchcase1Line "1"
	popup "2      2行" switchcase2Line "2"
	popup "3      3行" switchcase3Line "3"
	popup "4      4行" switchcase4Line "4"
	popup "5      5行" switchcase5Line "5"
	popup "6      6行" switchcase6Line "6"
	popup "7      7行" switchcase7Line "7"
	popup "8      8行" switchcase8Line "8"
	popup "9      9行" switchcase9Line "9"
end

