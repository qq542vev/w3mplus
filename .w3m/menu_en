	func "P  Search (New Tab)" GOTO "P" "file:///cgi-bin/w3mplus?action=get-register&key=%2F&run=redirect&tab=open-newtab"
	func "s  Send Email" GOTO "s" "file:///cgi-bin/w3mplus?action=get-register&key=%2F&run=redirect&redirect=mailto%3A%3Fbody%3D"
	func "S  Show" GOTO "S" "file:///cgi-bin/w3mplus?action=get-register&key=%2F&run=show"
