menu registerW
	func "o  Goto (Current Tab)" GOTO "o" "file:///cgi-bin/w3mplus?action=get-register&key=w&run=goto"
	func "t  Goto (New Tab)" GOTO "t" "file:///cgi-bin/w3mplus?action=get-register&key=w&run=goto&tab=newtab"
	func "p  Search (Current Tab)" GOTO "p" "file:///cgi-bin/w3mplus?action=get-register&key=w&run=redirect"
	func "P  Search (New Tab)" GOTO "P" "file:///cgi-bin/w3mplus?action=get-register&key=w&run=redirect&tab=open-newtab"
	func "s  Send Email" GOTO "s" "file:///cgi-bin/w3mplus?action=get-register&key=w&run=redirect&redirect=mailto%3A%3Fbody%3D"
	func "S  Show" GOTO "S" "file:///cgi-bin/w3mplus?action=get-register&key=w&run=show"
	func "y  URL Yank" COMMAND "y" "GOTO file:///cgi-bin/w3mplus?action=set-register&key=%2Bw&value=W3M_URL"
	func "Y  Word Yank" COMMAND "Y" "GOTO file:///cgi-bin/w3mplus?action=set-register&key=%2Bw&value=W3M_CURRENT_WORD"
end

