menu filter
	func  "!,h,l  Current line" GOTO "!hl" "file:///cgi-bin/w3mplus?action=select-line&subaction=filter&number=0"
	func  "j      1 line below" GOTO "j" "file:///cgi-bin/w3mplus?action=select-line&subaction=filter&number=%2B1"
	func  "k      1 line above" GOTO "k" "file:///cgi-bin/w3mplus?action=select-line&subaction=filter&number=-1"
	func  "g      All lines above" GOTO "g" "file:///cgi-bin/w3mplus?action=select-line&subaction=filter&number=1"
	func  "G      All lines below" GOTO "G" "file:///cgi-bin/w3mplus?action=select-line&subaction=filter&number=%24"
	func  "*      All lines" PIPE_BUF "*"
	nop   "----------------------"
	popup "1      1 line" filter1Line "1"
	popup "2      2 lines" filter2Line "2"
	popup "3      3 lines" filter3Line "3"
	popup "4      4 lines" filter4Line "4"
	popup "5      5 lines" filter5Line "5"
	popup "6      6 lines" filter6Line "6"
	popup "7      7 lines" filter7Line "7"
	popup "8      8 lines" filter8Line "8"
	popup "9      9 lines" filter9Line "9"
end

