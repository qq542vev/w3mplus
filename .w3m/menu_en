menu registerCurrent
	func "p  Search (Current Tab)" GOTO "p" "file:///cgi-bin/w3mplus?action=execute-env-var&run=redirect&variable=W3M_URL"
	func "P  Search (New Tab)" GOTO "P" "file:///cgi-bin/w3mplus?action=execute-env-var&run=redirect&variable=W3M_URL&tab=open-newtab"
	func "s  Send Email" GOTO "s" "file:///cgi-bin/w3mplus?action=execute-env-var&run=redirect&variable=W3M_URL&redirect=mailto%3A%3Fbody%3D"
	func "S  Show" GOTO "S" "file:///cgi-bin/w3mplus?action=execute-env-var&run=show&variable=W3M_URL"
end

