# Pentadactyl Key Map #

## Browsing ##

### Opening web pages ###

keymap "o" COMMAND "SET_OPTION default_url=0; GOTO"
keymap ":o" COMMAND "SET_OPTION default_url=0; GOTO"

keymap "t" COMMAND "SET_OPTION default_url=0; TAB_GOTO"
keymap ":t" COMMAND "SET_OPTION default_url=0; TAB_GOTO"

keymap "O" COMMAND "SET_OPTION default_url=1; GOTO"
keymap "T" COMMAND "SET_OPTION default_url=1; TAB_GOTO"

keymap "s" COMMAND "SET_OPTION dictcommand=file:///cgi-bin/w3mplus?action=dictWordSearch&query=; DICT_WORD"
keymap "S" COMMAND "SET_OPTION dictcommand=file:///cgi-bin/w3mplus?action=dictWordSearch&redirect=1&query=; DICT_WORD"

keymap "C-td" NEW_TAB

keymap "w" EXTERN "w3m ${WWW_HOME-about:home} #"
keymap "W" EXTERN

keymap "p" COMMAND "SET_OPTION dictcommand=file:///cgi-bin/w3mplus?action=dictWordSearch&query=; DICT_WORD_AT"
keymap "P" COMMAND "SET_OPTION dictcommand=file:///cgi-bin/w3mplus?action=dictWordSearch&redirect=1&query=; DICT_WORD_AT"
keymap "gP" COMMAND "SET_OPTION dictcommand=file:///cgi-bin/w3mplus?action=dictWordSearch&redirect=1&query=; DICT_WORD_AT; TAB_LEFT"

keymap "C-x" GOTO "file:///cgi-bin/w3mplus?action=uri&subaction=incrementURI&uri=%24%7BW3M_URL%7D"
keymap "C-a" GOTO "file:///cgi-bin/w3mplus?action=uri&subaction=decrementURI&uri=%24%7BW3M_URL%7D"

### History ###

keymap "C-o" UNDO
keymap "C-i" REDO

keymap ":j" SELECT_MENU

keymap "H" PREV
#keymap ^[^[[D PREV

keymap "L" NEXT
#keymap ^[^[[C NEXT

keymap ":B" COMMAND "PREV; PREV; PREV; PREV; PREV; PREV; PREV; PREV; PREV; PREV; PREV; PREV; PREV; PREV; PREV; PREV; PREV; PREV; PREV; PREV; PREV; PREV; PREV; PREV; PREV; PREV; PREV; PREV; PREV"
keymap ":F" COMMAND "NEXT; NEXT; NEXT; NEXT; NEXT; NEXT; NEXT; NEXT; NEXT; NEXT; NEXT; NEXT; NEXT; NEXT; NEXT; NEXT; NEXT; NEXT; NEXT; NEXT; NEXT; NEXT; NEXT; NEXT; NEXT; NEXT; NEXT; NEXT; NEXT"

keymap ":H" HISTORY

### Navigating ###

keymap "~" LOAD "~"
keymap "gh" GOTO "file:///cgi-bin/w3mplus?action=homepage"
keymap "gH" GOTO "file:///cgi-bin/w3mplus?action=homepage&redirect=1"

keymap "gu" GOTO "file:///cgi-bin/w3mplus?action=uri&subaction=parentPath&uri=%24%7BW3M_URL%7D"
keymap "gU" GOTO_RELATIVE "/"

### Reloading ###

keymap "r" RELOAD

### Stopping ###

keymap "C-c" STOP_IMAGE

### Writing ###

keymap ":w" DOWNLOAD
keymap ":W" PRINT

### Quitting ###

keymap "ZQ" EXIT
keymap ":x" EXIT

keymap "ZZ" QUIT
keymap ":q" QUIT

### The current directory ###

keymap ":P" EXEC_SHELL pwd

## Buffer ##

### Buffer information ###

keymap "C-g" LINE_INFO
keymap "gC-g" INFO

keymap "gf" VIEW
keymap "gF" COMMAND "VIEW; EDIT_SCREEN; VIEW"

### Motion commands ###

keymap "0" LINE_BEGIN
keymap "^" GOTO "file:///cgi-bin/w3mplus?action=moveColmun&percent=0"
keymap "$" LINE_END
keymap "g_" GOTO "file:///cgi-bin/w3mplus?action=moveColmun&percent=100"

keymap "gg" BEGIN
# Home key
keymap "^[[1~" BEGIN
keymap "^[[H" BEGIN

keymap "G" END
# End key
keymap "^[[4~" END
keymap "^[[F" END

keymap "%" MENU Percents

keymap "h" MOVE_LEFT1
keymap "LEFT" MOVE_LEFT1

keymap "j" MOVE_DOWN1
keymap "DOWN" MOVE_DOWN1

keymap "l" MOVE_RIGHT1
keymap "RIGHT" MOVE_RIGHT1

keymap "k" MOVE_UP1
keymap "UP" MOVE_UP1

keymap "C-e" UP
keymap "C-y" DOWN

keymap "C-d" NEXT_HALF_PAGE
keymap "C-u" PREV_HALF_PAGE

keymap "C-b" PREV_PAGE
keymap "M-SPC" PREV_PAGE
# Page up key
keymap "^[[5~" PREV_PAGE
keymap "^[[I" PREV_PAGE

keymap "C-f" NEXT_PAGE
keymap "SPC" NEXT_PAGE
# Page down key
keymap "^[[6~" NEXT_PAGE
keymap "^[[G" NEXT_PAGE

### Jumping to elements ###

keymap "TAB" NEXT_LINK
keymap "M-TAB" PREV_LINK

keymap "gi" COMMAND "SEARCH '\[..* \]'; MOVE_RIGHT1"

keymap "[[" SEARCH "[Pp][Rr][Ee][Vv]|<<|«|前へ"
keymap "]]" SEARCH "[Nn][Ee][Xx][Tt]|>>|»|次へ"

keymap "{" GOTO "file:///cgi-bin/w3mplus?action=moveParagraph&number=-1"
keymap "}" GOTO "file:///cgi-bin/w3mplus?action=moveParagraph&number=%2B1"

### Zooming ###

keymap "zi" GOTO "file:///cgi-bin/w3mplus?action=zoom&zoom=%2B20"
keymap "+" GOTO "file:///cgi-bin/w3mplus?action=zoom&zoom=%2B20"
keymap "zm" GOTO "file:///cgi-bin/w3mplus?action=zoom&zoom=%2B60"

keymap "zo" GOTO "file:///cgi-bin/w3mplus?action=zoom&zoom=-20"
keymap "-" GOTO "file:///cgi-bin/w3mplus?action=zoom&zoom=-20"
keymap "zr" GOTO "file:///cgi-bin/w3mplus?action=zoom&zoom=-60"

keymap "zz" GOTO "file:///cgi-bin/w3mplus?action=zoom&zoom=100"

### Working with frames ###

keymap ":f" FRAME

### Copying text ###

keymap "y" GOTO "file:///cgi-bin/w3mplus?action=yank&yank=%24%7BW3M_URL%7D"
keymap "Y" GOTO "file:///cgi-bin/w3mplus?action=yank&yank=%24%7BW3M_CURRENT_WORD%7D"

## Command Line mode ##

keymap "::" COMMAND

### Command-line completion ###

keymap ":TAB" MENU Commands

## Options ##

### Setting options ###

keymap ":s" SET_OPTION
keymap ":S" SETENV

### Setting w3m options ###

keymap ":p" OPTIONS

## Text find commands ##

keymap "/" ISEARCH
keymap "?" ISEARCH_BACK

keymap "n" SEARCH_NEXT
keymap "N" SEARCH_PREV

keymap "#" GOTO "file:///cgi-bin/w3mplus?action=wordFind&number=-1"
keymap "*" GOTO "file:///cgi-bin/w3mplus?action=wordFind&number=%2B1"

keymap "g#" GOTO "file:///cgi-bin/w3mplus?action=stringFind&number=-1"
keymap "g*" GOTO "file:///cgi-bin/w3mplus?action=stringFind&number=%2B1"

## Tabs and buffers ##

### Listing tabs ###

keymap "B" TAB_MENU

### Opening tabs ###

keymap "C-tt" TAB_GOTO "about:blank"

### Changing tabs ###

keymap "gt" NEXT_TAB
keymap "C-n" NEXT_TAB

keymap "gT" PREV_TAB
keymap "C-p" PREV_TAB

### Reordering tabs ###

keymap "C-tL" TAB_RIGHT
keymap "C-tH" TAB_LEFT

keymap "C-t0" COMMAND "TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT"
keymap "C-t$" COMMAND "TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT"

### Closing tabs ###

keymap "d" COMMAND "TAB_RIGHT; GOTO file:///cgi-bin/w3mplus?action=closeTab"
keymap "D" GOTO "file:///cgi-bin/w3mplus?action=closeTab"

keymap "C-to" COMMAND "TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; PREV_TAB; CLOSE_TAB; CLOSE_TAB; CLOSE_TAB; CLOSE_TAB; CLOSE_TAB; CLOSE_TAB; CLOSE_TAB; CLOSE_TAB; CLOSE_TAB; CLOSE_TAB; CLOSE_TAB; CLOSE_TAB; CLOSE_TAB; CLOSE_TAB; CLOSE_TAB; CLOSE_TAB; CLOSE_TAB; CLOSE_TAB; CLOSE_TAB; CLOSE_TAB;"

keymap "u" GOTO "file:///cgi-bin/w3mplus?action=undoTab"
keymap "U" GOTO "file:///cgi-bin/w3mplus?action=undoTab&count=-0"

keymap ":u" GOTO "file:///cgi-bin/w3mplus?action=undoTabs"

## Hints ##

keymap "f" LIST_MENU
keymap "F" COMMAND "NEW_TAB; LIST_MENU"

keymap ";;" MOVE_LIST_MENU
keymap ";?" LINE_INFO
keymap ";s" SAVE_LINK
keymap ";o" GOTO_LINK
keymap ";t" TAB_LINK
keymap ";b" TAB_GOTO "file:///cgi-bin/w3mplus?action=uri&subaction=prevTab&uri=%24%7BW3M_CURRENT_LINK%7D&redirect=2"
keymap ";w" EXTERN_LINK "w3m %s"
keymap ";O" GOTO "file:///cgi-bin/w3mplus?action=linkSetOpen"
keymap ";T" GOTO "file:///cgi-bin/w3mplus?action=linkSetTabopen"
keymap ";W" EXTERN_LINK
keymap ";a" GOTO "file:///cgi-bin/w3mplus?action=uri&subaction=addBookmark&uri=%24%7BW3M_CURRENT_LINK%7D"
keymap ";v" GOTO "file:///cgi-bin/w3mplus?action=uri&subaction=viewSource&uri=%24%7BW3M_CURRENT_LINK%7D"
keymap ";V" GOTO "file:///cgi-bin/w3mplus?action=uri&subaction=viewSourceExternally&uri=%24%7BW3M_CURRENT_LINK%7D"
keymap ";y" GOTO "file:///cgi-bin/w3mplus?action=yank&yank=%24%7BW3M_CURRENT_LINK%7D"
keymap ";Y" GOTO "file:///cgi-bin/w3mplus?action=yank&yank=%24%7BW3M_CURRENT_WORD%7D"
keymap ";c" GOTO "file:///cgi-bin/w3mplus?action=contextMenu"
keymap ";i" VIEW_IMAGE
keymap ";I" SAVE_IMAGE

keymap "g;" MENU ExtendedHints
keymap ";TAB" MENU ExtendedHints

## Keyboard shortcuts and commands ##

### Bypassing Pentadactyl ###

keymap "C-v" SET_OPTION "keymap_file=keymap-default-toggle"
keymap "M-z" SET_OPTION "keymap_file=keymap-default"

## Marks ##

### Bookmarks ###

keymap "a" ADD_BOOKMARK
keymap ":b" VIEW_BOOKMARK

### QuickMarks ###

keymap "M0" GOTO "file:///cgi-bin/w3mplus?action=setQuickMark&key=0"
