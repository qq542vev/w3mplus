keymap "MJ" GOTO "file:///cgi-bin/w3mplus?action=setQuickMark&key=J"
keymap "MK" GOTO "file:///cgi-bin/w3mplus?action=setQuickMark&key=K"
keymap "ML" GOTO "file:///cgi-bin/w3mplus?action=setQuickMark&key=L"
