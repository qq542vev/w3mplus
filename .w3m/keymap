## w3m UI ##

### Menus ###

keymap ":e" MENU Menu

### Dialogs ###

keymap ":d" DOWNLOAD_LIST

## Error and informational messages ##

keymap ":M" MSGS

## Privacy and sensitive information ##

### Cookie settings ###

keymap ":c" COOKIE

## Other ##

### Various commands ###

keymap "C-l" REDRAW
keymap ":!" EXEC_SHELL
keymap ":v" VERSION

## Online help ##

keymap ":h" HELP

### Uncategorized ###

keymap "i" EDIT_SCREEN
# Insert key
keymap "^[[2~" EDIT_SCREEN
keymap "^[[L" EDIT_SCREEN

# Vim Key Map #

keymap "z." CENTER_V

keymap "zh" LEFT
keymap "zLEFT" LEFT
keymap "zl" RIGHT
keymap "zRIGHT" RIGHT

keymap "gx" COMMAND "MARK_URL; GOTO_LINK"

keymap "V" GOTO "file:///cgi-bin/w3mplus?action=lineVisualMode"

keymap ":r" PIPE_SHELL

keymap "!" MENU "filter"
keymap "g~" MENU "switchcase"
keymap "g?" MENU "rot13"
keymap "g@" MENU "operatorFunc"
keymap "gq" MENU "formatPrg"
keymap "=" GOTO "file:///cgi-bin/w3mplus?action=equalPrg"

keymap "g:" GOTO_LINE

# Firefox Key Map #

# F1 key
keymap "^[OP" HELP
# F3 key
keymap "^[OR" SEARCH_NEXT
# Meta + F4 key
keymap "M-^[OS" EXIT
# F5 key
keymap "^[[15~" RELOAD
# F6 key
keymap "^[[17~" COMMAND "SET_OPTION default_url=1; GOTO"
# F7 key
keymap "^[[18~" EDIT_SCREEN
# F10 key
keymap "^[[21~" MENU Menu

keymap "M-f" MENU MenuFile
keymap "M-e" MENU MenuEdit
keymap "M-v" MENU MenuView
keymap "M-s" MENU MenuHistory
keymap "M-b" MENU MenuBookmarks
keymap "M-t" MENU MenuTools
keymap "M-h" MENU MenuHelp

keymap "DEL" PREV
