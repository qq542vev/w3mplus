# Vim Key Map #

keymap "z." CENTER_V

keymap "zh" LEFT
keymap "zLEFT" LEFT
keymap "zl" RIGHT
keymap "zRIGHT" RIGHT

keymap "gx" COMMAND "MARK_URL; GOTO_LINK"

keymap "C-tL" TAB_RIGHT
keymap "C-tH" TAB_LEFT

keymap "C-to" COMMAND "TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; PREV_TAB; CLOSE_TAB; CLOSE_TAB; CLOSE_TAB; CLOSE_TAB; CLOSE_TAB; CLOSE_TAB; CLOSE_TAB; CLOSE_TAB; CLOSE_TAB; CLOSE_TAB; CLOSE_TAB; CLOSE_TAB; CLOSE_TAB; CLOSE_TAB; CLOSE_TAB; CLOSE_TAB; CLOSE_TAB; CLOSE_TAB; CLOSE_TAB; CLOSE_TAB;"

keymap "#" GOTO "file:///cgi-bin/w3mplus?action=wordFindBack"
keymap "*" GOTO "file:///cgi-bin/w3mplus?action=wordFindFore"

keymap "g#" GOTO "file:///cgi-bin/w3mplus?action=stringFindBack"
keymap "g*" GOTO "file:///cgi-bin/w3mplus?action=stringFindFore"

keymap "V" COMMAND "PRINT /tmp/w3mplus_view_cache; GOTO file:///cgi-bin/w3mplus?action=lineVisualMode&file=/tmp/w3mplus_view_cache"

keymap ":r" PIPE_SHELL

keymap "!" MENU "filter"
keymap "g~" MENU "switchcase"
keymap "g?" MENU "rot13"
keymap "g@" MENU "operatorFunc"

# Firefox Key Map #

keymap "^[OP" HELP
keymap "M-^[OS" EXIT
keymap "^[[15~" RELOAD
keymap "^[[18~" EDIT_SCREEN

keymap "M-f" MENU MenuFile
keymap "M-e" MENU MenuEdit
keymap "M-v" MENU MenuView
keymap "M-s" HISTORY
keymap "M-b" MENU MenuBookmarks
keymap "M-t" MENU MenuTools
keymap "M-h" MENU MenuHelp

keymap "TAB" NEXT_LINK
keymap "M-TAB" PREV_LINK
keymap "DEL" PREV

keymap "s" COMMAND "SET_OPTION dictcommand=file:///cgi-bin/w3mplus?action=dictWordSearch&query=; DICT_WORD"
keymap "S" COMMAND "SET_OPTION dictcommand=file:///cgi-bin/w3mplus?action=dictWordSearch&redirect=1&query=; DICT_WORD"
keymap "g_" COMMAND "PRINT /tmp/w3mplus_view_cache; GOTO file:///cgi-bin/w3mplus?action=lineEnd&file=/tmp/w3mplus_view_cache"
