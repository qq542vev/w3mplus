## w3m UI ##

### Menus ###

keymap ":e" MENU Menu

### Dialogs ###

keymap ":d" DOWNLOAD_LIST

## Styling the GUI and web pages ##

keymap ":C" MENU MenuStyle

## Error and informational messages ##

keymap ":G" MSGS

## Privacy and sensitive information ##

### Cookie settings ###

keymap ":c" COOKIE

## Other ##

### Various commands ###

keymap "C-l" REDRAW
keymap ":!" EXEC_SHELL
keymap ":v" VERSION

## Online help ##

keymap ":h" HELP

keymap ":l" MENU LCommands

### Uncategorized ###

keymap "i" EDIT_SCREEN
# Insert key
keymap "^[[2~" EDIT_SCREEN
keymap "^[[L" EDIT_SCREEN

# Vim Key Map #

keymap "E" NEXT_WORD
keymap "B" PREV_WORD

keymap "+" GOTO "file:///cgi-bin/w3mplus?pass=$(PASS)&action=line-incriment&lineincriment=1&_action=line-begin-non-blake"
keymap "-" GOTO "file:///cgi-bin/w3mplus?pass=$(PASS)&action=line-incriment&lineincriment=-1&_action=line-begin-non-blake"

keymap "zt" GOTO "file:///cgi-bin/w3mplus?pass=$(PASS)&action=scroll-zt"
keymap "zb" GOTO "file:///cgi-bin/w3mplus?pass=$(PASS)&action=scroll-zb"

keymap "z." COMMAND "CENTER_V; GOTO file:///cgi-bin/w3mplus?pass=$(PASS)&action=line-begin-non-blake"
keymap "zz" CENTER_V

keymap "zh" LEFT
keymap "zLEFT" LEFT
keymap "zl" RIGHT
keymap "zRIGHT" RIGHT

keymap "zL" COMMAND "SHIFT_RIGHT; MOVE_LEFT; MOVE_RIGHT1"
keymap "zH" COMMAND "SHIFT_LEFT; MOVE_RIGHT; MOVE_LEFT1"

keymap "gj" MOVE_DOWN1
keymap "gk" MOVE_UP1
keymap "gx" COMMAND "MARK_URL; GOTO_LINK"

keymap "V" GOTO "file:///cgi-bin/w3mplus?pass=$(PASS)&action=visual-mode&subaction=yank"

keymap ":r" PIPE_SHELL
keymap ":\"" GOTO "file:///cgi-bin/w3mplus?pass=$(PASS)&action=show-register"

keymap "!" MENU "Filter"
keymap "K" GOTO "file:///cgi-bin/w3mplus?pass=$(PASS)&action=execute-env-var&subaction=keywordprg&variable=W3M_CURRENT_WORD"
keymap "g~" MENU "Switchcase"
keymap "g?" MENU "ROT13"
keymap "g@" MENU "Operatorfunc"
keymap "gq" MENU "Formatprg"
keymap "=" GOTO "file:///cgi-bin/w3mplus?pass=$(PASS)&action=equalprg"

keymap "g:" GOTO_LINE

keymap "C-z" SUSPEND
keymap "gM" CENTER_H
keymap ":L" SETENV
keymap ":S" EXEC_SHELL "${SHELL}"

