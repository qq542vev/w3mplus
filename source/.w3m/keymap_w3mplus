keymap "^[[31~" NULL
keymap "^[[32~" NULL
keymap "^[[33~" NULL
keymap "^[[34~" NULL

### Key used in w3m ###

keymap "^[[E" NULL
keymap "^[[L" NULL
keymap "^[[Z" NULL

# Defining a Pale Moon like keymap #

## Navigation ##

# Backspace key
keymap "DEL" PREV
keymap "ESC-DEL" NEXT
keymap "C-o" LOAD
# F5 key
keymap "^[[15~" RELOAD
keymap "C-r" RELOAD

## Current Page ##

keymap "TAB" NEXT_LINK
# Shift key + Tab key in xterm
keymap "^[[Z" PREV_LINK
keymap "ESC-TAB" PREV_LINK
# Page Down key
keymap "^[[6~" NEXT_PAGE
keymap "^[[G" NEXT_PAGE
keymap "SPC" NEXT_PAGE
# Page Up key
keymap "^[[5~" PREV_PAGE
keymap "^[[I" PREV_PAGE
keymap "ESC-SPC" PREV_PAGE
# End key
keymap "^[OF" END
keymap "^[[4~" END
keymap "^[[8~" END
keymap "^[[F" END
# Home key
keymap "^[OH" BEGIN
keymap "^[[1~" BEGIN
keymap "^[[7~" BEGIN
keymap "^[[H" BEGIN
keymap "C-p" PRINT
keymap "ESC-C-j" SAVE_LINK
keymap "ESC-C-m" SAVE_LINK
keymap "C-s" DOWNLOAD

## Editing ##

keymap "C-c" GOTO "file:///cgi-bin/w3mplus?pass=$(PASS)&action=execute-env-var&subaction=yank&variable=W3M_CURRENT_WORD"
keymap "C-v" COMMAND "SET_OPTION dictcommand=file:///cgi-bin/w3mplus?pass=$(PASS)&action=dict-word&designator=all&query=; DICT_WORD_AT"
keymap "C-y" REDO
keymap "C-z" UNDO

## Search ##

keymap "C-f" SEARCH
# F3 key
keymap "^[OR" SEARCH_NEXT
keymap "^[[13~" SEARCH_NEXT
keymap "C-g" SEARCH_NEXT
# F15 key
keymap "^[[28~" SEARCH_PREV
keymap "ESC-C-g" SEARCH_PREV
keymap "'" LIST_MENU
keymap "/" ISEARCH
keymap "C-k" COMMAND "SET_OPTION dictcommand=file:///cgi-bin/w3mplus?pass=$(PASS)&action=dict-word&designator=all&query=; DICT_WORD"
keymap "C-e" COMMAND "SET_OPTION dictcommand=file:///cgi-bin/w3mplus?pass=$(PASS)&action=dict-word&designator=all&tab=open-newtab&query=; DICT_WORD"
# F4 key
keymap "^[OS" MENU ContextMenuSearch
keymap "^[[14~" MENU ContextMenuSearch

## Windows & Tabs ##

keymap "C-w" COMMAND "TAB_RIGHT; GOTO file:///cgi-bin/w3mplus?pass=$(PASS)&action=close-tab"
keymap "ESC-C-w" QUIT
keymap "C-q" EXIT
keymap "C-t" TAB_GOTO "about:blank"
keymap "C-n" EXTERN "w3m ${WWW_HOME-about:home} #"
keymap "ESC-C-p" EXTERN "w3m-private --link './.w3mplus' -- 'about:private' #"
keymap "C-j" GOTO_LINK
keymap "C-m" GOTO_LINK
keymap "ESC-C-t" GOTO "file:///cgi-bin/w3mplus?pass=$(PASS)&action=undo-tab"
keymap "ESC-1" COMMAND "NEW_TAB; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; CLOSE_TAB"
keymap "ESC-2" COMMAND "NEW_TAB; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_RIGHT; TAB_RIGHT; CLOSE_TAB"
keymap "ESC-3" COMMAND "NEW_TAB; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; CLOSE_TAB"
keymap "ESC-4" COMMAND "NEW_TAB; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; CLOSE_TAB"
keymap "ESC-5" COMMAND "NEW_TAB; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; CLOSE_TAB"
keymap "ESC-6" COMMAND "NEW_TAB; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; CLOSE_TAB"
keymap "ESC-7" COMMAND "NEW_TAB; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; CLOSE_TAB"
keymap "ESC-8" COMMAND "NEW_TAB; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; CLOSE_TAB"
keymap "ESC-9" COMMAND "NEW_TAB; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; CLOSE_TAB"

## History ##

keymap "C-h" GOTO "file:///cgi-bin/w3mplus?pass=$(PASS)&action=sidebar-history"
keymap "ESC-C-h" HISTORY

## Bookmarks ##

keymap "C-d" ADD_BOOKMARK
keymap "C-b" GOTO "file:///cgi-bin/w3mplus?pass=$(PASS)&action=sidebar-bookmark"
keymap "ESC-C-b" BOOKMARK

## Tools ##

keymap "ESC-C-y" DOWNLOAD_LIST
# F12 key
keymap "^[[24~" MENU MenuWebDeveloper
keymap "ESC-C-k" PIPE_BUF
keymap "ESC-C-q" LOAD "Nn" "~/.w3m/request.log"
# F14 key
keymap "^[[27~" COMMAND
# F16 key
keymap "^[[29~" EXEC_SHELL
keymap "C-u" VIEW

## Miscellaneous ##

# F10 key
keymap "^[[21~" MENU Menu
# F7 key
keymap "^[[18~" EDIT_SCREEN
# F6 key
keymap "^[[17~" COMMAND "SET_OPTION default_url=1; GOTO"
keymap "ESC-d" COMMAND "SET_OPTION default_url=1; TAB_GOTO"
keymap "C-l" COMMAND "SET_OPTION default_url=1; GOTO"

## Menu Bar ##

keymap "ESC-f" MENU MenuFile
keymap "ESC-e" MENU MenuEdit
keymap "ESC-v" MENU MenuView
keymap "ESC-s" MENU MenuHistory
keymap "ESC-b" MENU MenuBookmarks
keymap "ESC-t" MENU MenuTools
keymap "ESC-h" MENU MenuHelp

## Other ##

keymap "LEFT" MOVE_LEFT
keymap "RIGHT" MOVE_RIGHT
keymap "UP" MOVE_UP
keymap "DOWN" MOVE_DOWN
keymap "ESC-C-d" SUSPEND
keymap "ESC-C-v" SET_OPTION "keymap_file=keymap_w3mplus"

# Defining a Pentadactyl like keymap #

## Browsing ##

### Opening web pages ###

keymap "o" COMMAND "SET_OPTION default_url=0; GOTO"
keymap ":o" COMMAND "SET_OPTION default_url=0; GOTO"

keymap "t" COMMAND "SET_OPTION default_url=0; TAB_GOTO"
keymap ":t" COMMAND "SET_OPTION default_url=0; TAB_GOTO"

keymap "O" COMMAND "SET_OPTION default_url=1; GOTO"
keymap "T" COMMAND "SET_OPTION default_url=1; TAB_GOTO"

keymap "s" COMMAND "SET_OPTION dictcommand=file:///cgi-bin/w3mplus?pass=$(PASS)&action=dict-word&designator=all&query=; DICT_WORD"
keymap "S" COMMAND "SET_OPTION dictcommand=file:///cgi-bin/w3mplus?pass=$(PASS)&action=dict-word&designator=all&tab=open-newtab&query=; DICT_WORD"

keymap "C-td" NEW_TAB

keymap "w" EXTERN "w3m ${WWW_HOME-about:home} #"
keymap "W" EXTERN

keymap "p" COMMAND "SET_OPTION dictcommand=file:///cgi-bin/w3mplus?pass=$(PASS)&action=dict-word&designator=all&query=; DICT_WORD_AT"
keymap "P" COMMAND "SET_OPTION dictcommand=file:///cgi-bin/w3mplus?pass=$(PASS)&action=dict-word&designator=all&tab=open-newtab&query=; DICT_WORD_AT"
keymap "gP" COMMAND "SET_OPTION dictcommand=file:///cgi-bin/w3mplus?pass=$(PASS)&action=dict-word&designator=all&tab=open-newtab&after=PREV_TAB&query=; DICT_WORD_AT"

keymap "C-x" GOTO "file:///cgi-bin/w3mplus?pass=$(PASS)&action=execute-env-var&subaction=increment-uri&variable=W3M_URL&number=-1"
keymap "C-a" GOTO "file:///cgi-bin/w3mplus?pass=$(PASS)&action=execute-env-var&subaction=increment-uri&variable=W3M_URL"

### History ###

keymap "C-o" UNDO
#keymap "C-i" REDO
keymap "ESC-C-o" REDO

keymap ":j" SELECT_MENU

keymap "H" PREV
keymap "L" NEXT

keymap ":H" HISTORY

### Navigating ###

keymap "~" LOAD "~"
keymap "gh" GOTO "file:///cgi-bin/w3mplus?pass=$(PASS)&action=homepage"
keymap "gH" GOTO "file:///cgi-bin/w3mplus?pass=$(PASS)&action=homepage&tab=open-newtab"

keymap "gu" GOTO "file:///cgi-bin/w3mplus?pass=$(PASS)&action=execute-env-var&subaction=parent-uripath&variable=W3M_URL"
keymap "gU" GOTO_RELATIVE "/"

### Reloading ###

keymap "r" RELOAD

### Stopping ###

keymap "C-c" STOP_IMAGE

### Writing ###

keymap ":w" DOWNLOAD
keymap ":W" PRINT

### Quitting ###

keymap "ZQ" EXIT
keymap ":x" EXIT

keymap "ZZ" QUIT
keymap ":q" QUIT

## Buffer ##

### Buffer information ###

keymap "C-g" LINE_INFO
keymap "gC-g" INFO

keymap "gf" VIEW
keymap "gF" COMMAND "VIEW; EDIT_SCREEN; VIEW"

### Motion commands ###

keymap "0" LINE_BEGIN
keymap "\^" GOTO "file:///cgi-bin/w3mplus?pass=$(PASS)&action=line-begin-non-blake"
keymap "$" LINE_END
keymap "g_" GOTO "file:///cgi-bin/w3mplus?pass=$(PASS)&action=line-end-non-blake"

keymap "gg" BEGIN
# Home key
keymap "^[OH" BEGIN
keymap "^[[1~" BEGIN
keymap "^[[7~" BEGIN
keymap "^[[H" BEGIN

keymap "G" END
# End key
keymap "^[OF" END
keymap "^[[4~" END
keymap "^[[8~" END
keymap "^[[F" END

keymap "%" MENU Percents

keymap "h" MOVE_LEFT
keymap "LEFT" MOVE_LEFT

keymap "j" MOVE_DOWN
keymap "DOWN" MOVE_DOWN

keymap "l" MOVE_RIGHT
keymap "RIGHT" MOVE_RIGHT

keymap "k" MOVE_UP
keymap "UP" MOVE_UP

keymap "C-e" UP
keymap "C-y" DOWN

keymap "C-d" NEXT_HALF_PAGE
keymap "C-u" PREV_HALF_PAGE

keymap "C-b" PREV_PAGE
keymap "ESC-SPC" PREV_PAGE
# Page Up key
keymap "^[[5~" PREV_PAGE
keymap "^[[I" PREV_PAGE

keymap "C-f" NEXT_PAGE
keymap "SPC" NEXT_PAGE
# Page Down key
keymap "^[[6~" NEXT_PAGE
keymap "^[[G" NEXT_PAGE

### Jumping to elements ###

keymap "TAB" NEXT_LINK

# Shift key + Tab key in xterm
keymap "^[[Z" PREV_LINK
keymap "ESC-TAB" PREV_LINK

keymap "gi" COMMAND "SEARCH '\[..* \]'; MOVE_RIGHT1"

keymap "[[" SEARCH "[Pp][Rr][Ee][Vv]|<<|«|前へ"
keymap "]]" SEARCH "[Nn][Ee][Xx][Tt]|>>|»|次へ"

keymap "{" GOTO "file:///cgi-bin/w3mplus?pass=$(PASS)&action=goto-paragraph&number=-1"
keymap "}" GOTO "file:///cgi-bin/w3mplus?pass=$(PASS)&action=goto-paragraph&number=%2B1"

### Zooming ###

keymap "ZI" GOTO "file:///cgi-bin/w3mplus?pass=$(PASS)&action=zoom&zoom=%2B%3D20"
keymap "ZM" GOTO "file:///cgi-bin/w3mplus?pass=$(PASS)&action=zoom&zoom=%2B%3D60"

keymap "ZO" GOTO "file:///cgi-bin/w3mplus?pass=$(PASS)&action=zoom&zoom=-%3D20"
keymap "ZR" GOTO "file:///cgi-bin/w3mplus?pass=$(PASS)&action=zoom&zoom=-%3D60"

keymap "zZ" GOTO "file:///cgi-bin/w3mplus?pass=$(PASS)&action=zoom&zoom=100"

keymap ":z" MENU Zoom

### Working with frames ###

keymap ":f" FRAME

### Copying text ###

keymap "y" GOTO "file:///cgi-bin/w3mplus?pass=$(PASS)&action=execute-env-var&subaction=yank&variable=W3M_URL"
keymap "Y" GOTO "file:///cgi-bin/w3mplus?pass=$(PASS)&action=execute-env-var&subaction=yank&variable=W3M_CURRENT_WORD"

## Command Line mode ##

keymap "::" COMMAND

### Command-line completion ###

keymap ":TAB" MENU Commands

## Options ##

### Setting options ###

keymap ":s" SET_OPTION

### Setting w3m options ###

keymap ":p" OPTIONS

## Text find commands ##

keymap "/" ISEARCH
keymap "?" ISEARCH_BACK

keymap "n" SEARCH_NEXT
keymap "N" SEARCH_PREV

keymap "#" GOTO "file:///cgi-bin/w3mplus?pass=$(PASS)&action=find-word&number=-1"
keymap "*" GOTO "file:///cgi-bin/w3mplus?pass=$(PASS)&action=find-word&number=%2B1"

keymap "g#" GOTO "file:///cgi-bin/w3mplus?pass=$(PASS)&action=find-string&number=-1"
keymap "g*" GOTO "file:///cgi-bin/w3mplus?pass=$(PASS)&action=find-string&number=%2B1"

## Tabs and buffers ##

### Listing tabs ###

keymap "b" TAB_MENU

### Opening tabs ###

keymap "C-tt" TAB_GOTO "about:blank"

### Changing tabs ###

keymap "gt" NEXT_TAB
keymap "C-n" NEXT_TAB

keymap "gT" PREV_TAB
keymap "C-p" PREV_TAB

keymap "g0" COMMAND "NEW_TAB; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; CLOSE_TAB"
keymap "g$" COMMAND "NEW_TAB; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; CLOSE_TAB"

### Reordering tabs ###

keymap "C-tL" TAB_RIGHT
keymap "C-tH" TAB_LEFT

keymap "C-t0" COMMAND "TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT"
keymap "C-t$" COMMAND "TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT"

### Closing tabs ###

keymap "d" COMMAND "TAB_RIGHT; GOTO file:///cgi-bin/w3mplus?pass=$(PASS)&action=close-tab"
keymap "D" GOTO "file:///cgi-bin/w3mplus?pass=$(PASS)&action=close-tab"

keymap "C-to" COMMAND "TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; PREV_TAB; CLOSE_TAB; CLOSE_TAB; CLOSE_TAB; CLOSE_TAB; CLOSE_TAB; CLOSE_TAB; CLOSE_TAB; CLOSE_TAB; CLOSE_TAB; CLOSE_TAB; CLOSE_TAB; CLOSE_TAB; CLOSE_TAB; CLOSE_TAB; CLOSE_TAB; CLOSE_TAB; CLOSE_TAB; CLOSE_TAB; CLOSE_TAB; CLOSE_TAB;"

keymap "u" GOTO "file:///cgi-bin/w3mplus?pass=$(PASS)&action=undo-tab"
keymap "U" GOTO "file:///cgi-bin/w3mplus?pass=$(PASS)&action=undo-tab&count=-0"

keymap ":u" GOTO "file:///cgi-bin/w3mplus?pass=$(PASS)&action=show-tab-history"

## Hints ##

keymap "f" LIST_MENU
keymap "F" COMMAND "NEW_TAB; LIST_MENU"

keymap ";;" MOVE_LIST_MENU
keymap ";?" LINE_INFO
keymap ";s" SAVE_LINK
keymap ";o" GOTO_LINK
keymap ";t" TAB_LINK
keymap ";b" GOTO "file:///cgi-bin/w3mplus?pass=$(PASS)&action=execute-env-var&subaction=goto&tab=open-newtab&after=PREV_TAB&variable=W3M_CURRENT_LINK"
keymap ";w" EXTERN_LINK "w3m %s"
keymap ";O" GOTO "file:///cgi-bin/w3mplus?pass=$(PASS)&action=link-goto"
keymap ";T" GOTO "file:///cgi-bin/w3mplus?pass=$(PASS)&action=link-tabgoto"
keymap ";W" EXTERN_LINK
keymap ";a" GOTO "file:///cgi-bin/w3mplus?pass=$(PASS)&action=execute-env-var&subaction=goto&after=ADD_BOOKMARK&variable=W3M_CURRENT_LINK"
keymap ";S" GOTO "file:///cgi-bin/w3mplus?pass=$(PASS)&action=execute-env-var&subaction=redirect&variable=W3M_CURRENT_LINK"
keymap ";v" GOTO "file:///cgi-bin/w3mplus?pass=$(PASS)&action=execute-env-var&subaction=goto&after=VIEW&variable=W3M_CURRENT_LINK"
keymap ";V" GOTO "file:///cgi-bin/w3mplus?pass=$(PASS)&action=execute-env-var&subaction=goto&after=COMMAND%20VIEW%3B%20EDIT_SCREEN%3B%20VIEW&variable=W3M_CURRENT_LINK"
keymap ";y" GOTO "file:///cgi-bin/w3mplus?pass=$(PASS)&action=execute-env-var&subaction=yank&variable=W3M_CURRENT_LINK"
keymap ";Y" GOTO "file:///cgi-bin/w3mplus?pass=$(PASS)&action=execute-env-var&subaction=yank&variable=W3M_CURRENT_WORD"
keymap ";c" GOTO "file:///cgi-bin/w3mplus?pass=$(PASS)&action=context-menu"
keymap ";i" VIEW_IMAGE
keymap ";I" SAVE_IMAGE

keymap "g;" MENU ExtendedHints
keymap ";TAB" MENU ExtendedHints

## Keyboard shortcuts and commands ##

### Bypassing Pentadactyl ###

keymap "ESC-b" COMMAND "SET_OPTION default_url=0; SET_OPTION dictcommand=file:///cgi-bin/w3mplus?pass=$(PASS)&action=dict-word&designator=all&query=; SET_OPTION keymap_file=keymap_builtin-onetime"
keymap "ESC-C-b" COMMAND "SET_OPTION default_url=0; SET_OPTION dictcommand=file:///cgi-bin/w3mplus?pass=$(PASS)&action=dict-word&designator=all&query=; SET_OPTION keymap_file=keymap_builtin"
keymap "C-v" SET_OPTION "keymap_file=keymap_palemoon-onetime"
keymap "ESC-C-v" SET_OPTION "keymap_file=keymap_palemoon"

