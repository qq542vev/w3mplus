# Defining a Pale Moon like keymap #

## Navigation ##

# Backspace key
keymap "DEL" COMMAND "SET_OPTION keymap_file=keymap_w3mplus; PREV"
keymap "ESC-DEL" COMMAND "SET_OPTION keymap_file=keymap_w3mplus; NEXT"
keymap "C-o" COMMAND "SET_OPTION keymap_file=keymap_w3mplus; LOAD"
# F5 key
keymap "^[[15~" COMMAND "SET_OPTION keymap_file=keymap_w3mplus; RELOAD"
keymap "C-r" COMMAND "SET_OPTION keymap_file=keymap_w3mplus; RELOAD"

## Current Page ##

keymap "TAB" COMMAND "SET_OPTION keymap_file=keymap_w3mplus; NEXT_LINK"
# Shift key + Tab key in xterm
keymap "^[[Z" COMMAND "SET_OPTION keymap_file=keymap_w3mplus; PREV_LINK"
keymap "ESC-TAB" COMMAND "SET_OPTION keymap_file=keymap_w3mplus; PREV_LINK"
# Page Down key
keymap "^[[6~" COMMAND "SET_OPTION keymap_file=keymap_w3mplus; NEXT_PAGE"
keymap "^[[G" COMMAND "SET_OPTION keymap_file=keymap_w3mplus; NEXT_PAGE"
keymap "SPC" COMMAND "SET_OPTION keymap_file=keymap_w3mplus; NEXT_PAGE"
# Page Up key
keymap "^[[5~" COMMAND "SET_OPTION keymap_file=keymap_w3mplus; PREV_PAGE"
keymap "^[[I" COMMAND "SET_OPTION keymap_file=keymap_w3mplus; PREV_PAGE"
keymap "ESC-SPC" COMMAND "SET_OPTION keymap_file=keymap_w3mplus; PREV_PAGE"
# End key
keymap "^[OF" COMMAND "SET_OPTION keymap_file=keymap_w3mplus; END"
keymap "^[[4~" COMMAND "SET_OPTION keymap_file=keymap_w3mplus; END"
keymap "^[[8~" COMMAND "SET_OPTION keymap_file=keymap_w3mplus; END"
keymap "^[[F" COMMAND "SET_OPTION keymap_file=keymap_w3mplus; END"
# Home key
keymap "^[OH" COMMAND "SET_OPTION keymap_file=keymap_w3mplus; BEGIN"
keymap "^[[1~" COMMAND "SET_OPTION keymap_file=keymap_w3mplus; BEGIN"
keymap "^[[7~" COMMAND "SET_OPTION keymap_file=keymap_w3mplus; BEGIN"
keymap "^[[H" COMMAND "SET_OPTION keymap_file=keymap_w3mplus; BEGIN"
keymap "C-p" COMMAND "SET_OPTION keymap_file=keymap_w3mplus; PRINT"
keymap "ESC-C-j" COMMAND "SET_OPTION keymap_file=keymap_w3mplus; SAVE_LINK"
keymap "ESC-C-m" COMMAND "SET_OPTION keymap_file=keymap_w3mplus; SAVE_LINK"
keymap "C-s" COMMAND "SET_OPTION keymap_file=keymap_w3mplus; DOWNLOAD"

## Editing ##

keymap "C-c" COMMAND "SET_OPTION keymap_file=keymap_w3mplus; GOTO file:///cgi-bin/w3mplus?pass=$(PASS)&action=execute-env-var&subaction=yank&variable=W3M_CURRENT_WORD"
keymap "C-v" COMMAND "SET_OPTION keymap_file=keymap_w3mplus; SET_OPTION dictcommand=file:///cgi-bin/w3mplus?pass=$(PASS)&action=dict-word&designator=all&query=; DICT_WORD_AT"
keymap "C-y" COMMAND "SET_OPTION keymap_file=keymap_w3mplus; REDO"
keymap "C-z" COMMAND "SET_OPTION keymap_file=keymap_w3mplus; UNDO"

## Search ##

keymap "C-f" COMMAND "SET_OPTION keymap_file=keymap_w3mplus; SEARCH"
# F3 key
keymap "^[OR" COMMAND "SET_OPTION keymap_file=keymap_w3mplus; SEARCH_NEXT"
keymap "^[[13~" COMMAND "SET_OPTION keymap_file=keymap_w3mplus; SEARCH_NEXT"
keymap "C-g" COMMAND "SET_OPTION keymap_file=keymap_w3mplus; SEARCH_NEXT"
# F15 key
keymap "^[[28~" COMMAND "SET_OPTION keymap_file=keymap_w3mplus; SEARCH_PREV"
keymap "ESC-C-g" COMMAND "SET_OPTION keymap_file=keymap_w3mplus; SEARCH_PREV"
keymap "/" COMMAND "SET_OPTION keymap_file=keymap_w3mplus; ISEARCH"
keymap "C-k" COMMAND "SET_OPTION keymap_file=keymap_w3mplus; SET_OPTION dictcommand=file:///cgi-bin/w3mplus?pass=$(PASS)&action=dict-word&designator=all&query=; DICT_WORD"
keymap "C-e" COMMAND "SET_OPTION keymap_file=keymap_w3mplus; SET_OPTION dictcommand=file:///cgi-bin/w3mplus?pass=$(PASS)&action=dict-word&designator=all&tab=open-newtab&query=; DICT_WORD"
# F4 key
keymap "^[OS" COMMAND "SET_OPTION keymap_file=keymap_w3mplus; MENU ContextMenuSearch"
keymap "^[[14~" COMMAND "SET_OPTION keymap_file=keymap_w3mplus; MENU ContextMenuSearch"

## Windows & Tabs ##

keymap "C-w" COMMAND "SET_OPTION keymap_file=keymap_w3mplus; TAB_RIGHT; GOTO file:///cgi-bin/w3mplus?pass=$(PASS)&action=close-tab"
keymap "ESC-C-w" COMMAND "SET_OPTION keymap_file=keymap_w3mplus; QUIT"
keymap "C-q" COMMAND "SET_OPTION keymap_file=keymap_w3mplus; EXIT"
keymap "C-t" COMMAND "SET_OPTION keymap_file=keymap_w3mplus; TAB_GOTO about:blank"
keymap "C-n" COMMAND "SET_OPTION keymap_file=keymap_w3mplus; EXTERN \"w3m ${WWW_HOME-about:home} #\""
keymap "ESC-C-p" COMMAND "SET_OPTION keymap_file=keymap_w3mplus; EXTERN \"w3m-private --link './.w3m/bookmark.html' --link './.w3mplus' -- 'about:private' #\""
keymap "C-j" COMMAND "SET_OPTION keymap_file=keymap_w3mplus; GOTO_LINK"
keymap "C-m" COMMAND "SET_OPTION keymap_file=keymap_w3mplus; GOTO_LINK"
keymap "ESC-C-t" COMMAND "SET_OPTION keymap_file=keymap_w3mplus; GOTO file:///cgi-bin/w3mplus?pass=$(PASS)&action=undo-tab"
keymap "ESC-1" COMMAND "SET_OPTION keymap_file=keymap_w3mplus; NEW_TAB; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; CLOSE_TAB"
keymap "ESC-2" COMMAND "SET_OPTION keymap_file=keymap_w3mplus; NEW_TAB; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_RIGHT; TAB_RIGHT; CLOSE_TAB"
keymap "ESC-3" COMMAND "SET_OPTION keymap_file=keymap_w3mplus; NEW_TAB; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; CLOSE_TAB"
keymap "ESC-4" COMMAND "SET_OPTION keymap_file=keymap_w3mplus; NEW_TAB; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; CLOSE_TAB"
keymap "ESC-5" COMMAND "SET_OPTION keymap_file=keymap_w3mplus; NEW_TAB; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; CLOSE_TAB"
keymap "ESC-6" COMMAND "SET_OPTION keymap_file=keymap_w3mplus; NEW_TAB; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; CLOSE_TAB"
keymap "ESC-7" COMMAND "SET_OPTION keymap_file=keymap_w3mplus; NEW_TAB; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; CLOSE_TAB"
keymap "ESC-8" COMMAND "SET_OPTION keymap_file=keymap_w3mplus; NEW_TAB; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_LEFT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; CLOSE_TAB"
keymap "ESC-9" COMMAND "SET_OPTION keymap_file=keymap_w3mplus; NEW_TAB; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; TAB_RIGHT; CLOSE_TAB"

## History ##

keymap "C-h" COMMAND "SET_OPTION keymap_file=keymap_w3mplus; GOTO file:///cgi-bin/w3mplus?pass=$(PASS)&action=sidebar-history"
keymap "ESC-C-h" COMMAND "SET_OPTION keymap_file=keymap_w3mplus; HISTORY"

## Bookmarks ##

keymap "C-d" COMMAND "SET_OPTION keymap_file=keymap_w3mplus; ADD_BOOKMARK"
keymap "C-b" COMMAND "SET_OPTION keymap_file=keymap_w3mplus; GOTO file:///cgi-bin/w3mplus?pass=$(PASS)&action=sidebar-bookmark"
keymap "ESC-C-b" COMMAND "SET_OPTION keymap_file=keymap_w3mplus; BOOKMARK"

## Tools ##

keymap "ESC-C-y" COMMAND "SET_OPTION keymap_file=keymap_w3mplus; DOWNLOAD_LIST"
# F12 key
keymap "^[[24~" COMMAND "SET_OPTION keymap_file=keymap_w3mplus; MENU MenuWebDeveloper"
keymap "ESC-C-k" COMMAND "SET_OPTION keymap_file=keymap_w3mplus; PIPE_BUF"
keymap "ESC-C-q" COMMAND "SET_OPTION keymap_file=keymap_w3mplus; LOAD ~/.w3m/request.log"
# F14 key
keymap "^[[27~" COMMAND "SET_OPTION keymap_file=keymap_w3mplus; COMMAND"
# F16 key
keymap "^[[29~" COMMAND "SET_OPTION keymap_file=keymap_w3mplus; EXEC_SHELL"
keymap "C-u" COMMAND "SET_OPTION keymap_file=keymap_w3mplus; VIEW"

## Miscellaneous ##

# F11 key
keymap "^[[23~" COMMAND "SET_OPTION keymap_file=keymap_w3mplus; SET_OPTION \"editor=${PAGER:-less --}\"; EDIT_SCREEN; SET_OPTION \"editor=${VISUAL:-${EDITOR:-vi --}}\""
# F10 key
keymap "^[[21~" COMMAND "SET_OPTION keymap_file=keymap_w3mplus; MENU Menu"
# F7 key
keymap "^[[18~" COMMAND "SET_OPTION keymap_file=keymap_w3mplus; EDIT_SCREEN"
# F6 key
keymap "^[[17~" COMMAND "SET_OPTION keymap_file=keymap_w3mplus; SET_OPTION default_url=1; GOTO"
keymap "ESC-d" COMMAND "SET_OPTION keymap_file=keymap_w3mplus; SET_OPTION default_url=1; TAB_GOTO"
keymap "C-l" COMMAND "SET_OPTION keymap_file=keymap_w3mplus; SET_OPTION default_url=1; GOTO"
# F1 key
keymap "^[OP" COMMAND "SET_OPTION keymap_file=keymap_w3mplus; HELP"
keymap "^[[11~" COMMAND "SET_OPTION keymap_file=keymap_w3mplus; HELP"

## Menu Bar ##

keymap "ESC-f" COMMAND "SET_OPTION keymap_file=keymap_w3mplus; MENU MenuFile"
keymap "ESC-e" COMMAND "SET_OPTION keymap_file=keymap_w3mplus; MENU MenuEdit"
keymap "ESC-v" COMMAND "SET_OPTION keymap_file=keymap_w3mplus; MENU MenuView"
keymap "ESC-s" COMMAND "SET_OPTION keymap_file=keymap_w3mplus; MENU MenuHistory"
keymap "ESC-b" COMMAND "SET_OPTION keymap_file=keymap_w3mplus; MENU MenuBookmarks"
keymap "ESC-t" COMMAND "SET_OPTION keymap_file=keymap_w3mplus; MENU MenuTools"
keymap "ESC-h" COMMAND "SET_OPTION keymap_file=keymap_w3mplus; MENU MenuHelp"

## Other ##

keymap "LEFT" COMMAND "SET_OPTION keymap_file=keymap_w3mplus; MOVE_LEFT"
keymap "RIGHT" COMMAND "SET_OPTION keymap_file=keymap_w3mplus; MOVE_RIGHT"
keymap "UP" COMMAND "SET_OPTION keymap_file=keymap_w3mplus; MOVE_UP"
keymap "DOWN" COMMAND "SET_OPTION keymap_file=keymap_w3mplus; MOVE_DOWN"
keymap "ESC-C-d" COMMAND "SET_OPTION keymap_file=keymap_w3mplus; SUSPEND"
